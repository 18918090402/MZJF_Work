    <sql>SELECT &#xd;&#xa;  &#x24;&#x7b;T_1_0&#x7d; AS STAT_DATE,&#xd;&#xa;  oo.&#x60;order_no&#x60; AS &#x27;ORDER_NO&#x27;,&#xd;&#xa;  oo.&#x60;bo_id&#x60; AS &#x27;APPLY_NO&#x27;,&#xd;&#xa;  oud.&#x60;name&#x60; AS &#x27;APPLY_NAME&#x27;,&#xd;&#xa;  CASE&#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;1&#x27; &#xd;&#xa;    THEN &#x27;&#x5f85;&#x63d0;&#x4ea4;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;2&#x27; &#xd;&#xa;    THEN &#x27;&#x5f85;&#x5ba1;&#x6838;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;3&#x27; &#xd;&#xa;    THEN &#x27;&#x521d;&#x5ba1;&#x901a;&#x8fc7;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;4&#x27; &#xd;&#xa;    THEN &#x27;&#x590d;&#x5ba1;&#x901a;&#x8fc7;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;5&#x27; &#xd;&#xa;    THEN &#x27;&#x5ba1;&#x6279;&#x901a;&#x8fc7;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;6&#x27; &#xd;&#xa;    THEN &#x27;&#x5f85;&#x653e;&#x6b3e;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;7&#x27; &#xd;&#xa;    THEN &#x27;&#x5df2;&#x653e;&#x6b3e;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;8&#x27; &#xd;&#xa;    THEN &#x27;&#x5df2;&#x7ed3;&#x6e05;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;9&#x27; &#xd;&#xa;    THEN &#x27;&#x53d6;&#x6d88;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;10&#x27; &#xd;&#xa;    THEN &#x27;&#x9000;&#x6b3e;&#x4e2d;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;11&#x27; &#xd;&#xa;    THEN &#x27;&#x9000;&#x6b3e;&#x5b8c;&#x6bd5;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;12&#x27; &#xd;&#xa;    THEN &#x27;&#x590d;&#x5ba1;&#x56de;&#x9000;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;13&#x27; &#xd;&#xa;    THEN &#x27;&#x9ad8;&#x7ea7;&#x590d;&#x5ba1;&#x56de;&#x9000;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;14&#x27; &#xd;&#xa;    THEN &#x27;&#x5ba1;&#x6838;&#x4e0d;&#x901a;&#x8fc7;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;15&#x27; &#xd;&#xa;    THEN &#x27;&#x521d;&#x5ba1;&#x56de;&#x9000;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;16&#x27; &#xd;&#xa;    THEN &#x27;&#x5904;&#x7406;&#x4e2d;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;17&#x27; &#xd;&#xa;    THEN &#x27;&#x7528;&#x6237;&#x786e;&#x8ba4;&#x27; &#xd;&#xa;    WHEN oo.&#x60;status&#x60; &#x3d; &#x27;18&#x27; &#xd;&#xa;    THEN &#x27;&#x7528;&#x6237;&#x63d0;&#x4ea4;&#x4e2d;&#x27; &#xd;&#xa;  END AS &#x27;ORDER_STATUS&#x27;,&#xd;&#xa;  re_table.RETURN_REMARK ,&#xd;&#xa;  re_table.REFUSE_REASON,&#xd;&#xa;  oud.idcard AS &#x27;IDCARD&#x27;,&#xd;&#xa;  CASE&#xd;&#xa;    WHEN oud.&#x60;sex&#x60; &#x3d; &#x27;1&#x27; &#xd;&#xa;    THEN &#x27;&#x7537;&#x27; &#xd;&#xa;    WHEN oud.&#x60;sex&#x60; &#x3d; &#x27;2&#x27; &#xd;&#xa;    THEN &#x27;&#x5973;&#x27; &#xd;&#xa;  END AS &#x27;SEX&#x27; ,&#xd;&#xa;  CASE&#xd;&#xa;    WHEN oud.academic &#x3d; &#x27;1&#x27; &#xd;&#xa;    THEN &#x27;&#x521d;&#x4e2d;&#x53ca;&#x4ee5;&#x4e0b;&#x27; &#xd;&#xa;    WHEN oud.academic &#x3d; &#x27;2&#x27; &#xd;&#xa;    THEN &#x27;&#x4e2d;&#x4e13;_&#x9ad8;&#x4e2d;&#x27; &#xd;&#xa;    WHEN oud.academic &#x3d; &#x27;3&#x27; &#xd;&#xa;    THEN &#x27;&#x5927;&#x4e13;&#x27; &#xd;&#xa;    WHEN oud.academic &#x3d; &#x27;4&#x27; &#xd;&#xa;    THEN &#x27;&#x672c;&#x79d1;&#x27; &#xd;&#xa;    WHEN oud.academic &#x3d; &#x27;5&#x27; &#xd;&#xa;    THEN &#x27;&#x7855;&#x58eb;&#x7814;&#x7a76;&#x751f;&#x27; &#xd;&#xa;    WHEN oud.academic &#x3d; &#x27;6&#x27; &#xd;&#xa;    THEN &#x27;&#x535a;&#x58eb;&#x7814;&#x7a76;&#x751f;&#x27; &#xd;&#xa;  END AS &#x27;EDUCATION&#x27; ,&#xd;&#xa;  ou.mobile AS &#x27;MOBILE&#x27;,&#xd;&#xa;  oud.email AS &#x27;EMAIL&#x27;,&#xd;&#xa;  CASE&#xd;&#xa;    WHEN oud.work_position &#x3d; &#x27;1&#x27; &#xd;&#xa;    THEN &#x27;&#x9ad8;&#x7ea7;&#x7ba1;&#x7406;&#x4eba;&#x5458;&#x27; &#xd;&#xa;    WHEN oud.work_position &#x3d; &#x27;2&#x27; &#xd;&#xa;    THEN &#x27;&#x4e2d;&#x7ea7;&#x7ba1;&#x7406;&#x4eba;&#x5458;&#x27; &#xd;&#xa;    WHEN oud.work_position &#x3d; &#x27;3&#x27; &#xd;&#xa;    THEN &#x27;&#x529e;&#x516c;&#x7c7b;&#x5458;&#x5de5;&#x27; &#xd;&#xa;    WHEN oud.work_position &#x3d; &#x27;4&#x27; &#xd;&#xa;    THEN &#x27;&#x540e;&#x52e4;&#x7c7b;&#x5458;&#x5de5;&#x27; &#xd;&#xa;    WHEN oud.work_position &#x3d; &#x27;5&#x27; &#xd;&#xa;    THEN &#x27;&#x64cd;&#x4f5c;&#x7c7b;&#x5458;&#x5de5;&#x27; &#xd;&#xa;    WHEN oud.work_position &#x3d; &#x27;6&#x27; &#xd;&#xa;    THEN &#x27;&#x670d;&#x52a1;&#x7c7b;&#x5458;&#x5de5;&#x27; &#xd;&#xa;    WHEN oud.work_position &#x3d; &#x27;7&#x27; &#xd;&#xa;    THEN &#x27;&#x9500;&#x552e;&#x7c7b;&#x5458;&#x5de5;&#x27; &#xd;&#xa;    WHEN oud.work_position &#x3d; &#x27;8&#x27; &#xd;&#xa;    THEN &#x27;&#x65e0;&#x27; &#xd;&#xa;  END AS &#x27;POSITION&#x27;,&#xd;&#xa;  oud.company_name AS &#x27;COMPANY_NAME&#x27;,&#xd;&#xa;  r2.name AS &#x27;CURRENT_PROVINCE&#x27;,&#xd;&#xa;  r1.name AS &#x27;CURRENT_CITY&#x27;,&#xd;&#xa;  oo.create_time AS &#x27;CREATE_TIME&#x27;,&#xd;&#xa;  oo.order_begin_date AS &#x27;ORDER_BEGIN_DATE&#x27;,&#xd;&#xa;  oo.pass_time AS &#x27;PASS_TIME&#x27;,&#xd;&#xa;  oo.shop_confirm_time AS &#x27;LENDING_TIME&#x27;,&#xd;&#xa;  oo.principal AS &#x27;PRACTICAL_MONEY&#x27;,&#xd;&#xa;  oo.&#x60;total_parts&#x60; AS &#x27;TOTAL_PARTS&#x27;,&#xd;&#xa;  oo.channel_id AS &#x27;CHANNEL&#x27;,&#xd;&#xa;  oud.use_source AS &#x27;SOURCE&#x27;,&#xd;&#xa;  oo.is_call AS &#x27;PHONECALL_FLAG&#x27;,&#xd;&#xa;  ou.salary AS &#x22;FAMILY_INCOME_AMT&#x22;,&#xd;&#xa;  ou.is_qizha AS &#x27;FRAUD_FLAG&#x27;,&#xd;&#xa;  oo.shop_id AS &#x27;SHOP_ID&#x27;,&#xd;&#xa;  oo.shop_name AS &#x22;MERCHANT_NAME&#x22;,&#xd;&#xa;  &#x27;&#x27; AS &#x27;MERCHANT_TYPE&#x27;,&#xd;&#xa;  ouh.final_score AS &#x27;SCORE&#x27;,&#xd;&#xa;  ou.&#x60;honor_level&#x60; AS &#x27;LEVEL&#x27;,&#xd;&#xa;  oo.&#x60;apply_amount&#x60; AS &#x27;APPLY_AMOUNT&#x27;,&#xd;&#xa;  oo.&#x60;apply_parts&#x60; AS &#x27;APPLY_PARTS&#x27;,&#xd;&#xa;  sdi.&#x60;dic_item_name&#x60; AS &#x27;DIC_ITEM&#x27;,&#xd;&#xa;  &#x27;SYS&#x27; AS &#x27;DW_CREATE_BY&#x27;,&#xd;&#xa;  SYSDATE&#x28;&#x29; AS &#x27;DW_CREATE_TIME&#x27;,&#xd;&#xa;  &#x27;SYS&#x27; AS &#x27;DW_UPDATE_BY&#x27;,&#xd;&#xa;  SYSDATE&#x28;&#x29; AS &#x27;DW_UPDATE_TIME&#x27; &#xd;&#xa;FROM&#xd;&#xa;  &#x60;finance_order&#x60;.&#x60;ORD_ORDER&#x60; oo &#xd;&#xa;  JOIN finance_order.&#x60;ORD_USER&#x60; ou &#xd;&#xa;    ON oo.&#x60;create_user&#x60; &#x3d; ou.&#x60;id&#x60; &#xd;&#xa;  LEFT JOIN finance_order.ORD_USER_DETAIL oud &#xd;&#xa;    ON oo.create_user &#x3d; oud.user_id &#xd;&#xa;LEFT JOIN risk.&#x60;region&#x60; r1 ON oud.&#x60;city_code&#x60;&#x3d;r1.&#x60;code&#x60;&#xd;&#xa;    LEFT JOIN risk.&#x60;region&#x60; r2 ON oud.&#x60;province_code&#x60;&#x3d;r2.&#x60;code&#x60;&#xd;&#xa;  LEFT JOIN &#xd;&#xa;    &#x28;SELECT &#xd;&#xa;      order_id,&#xd;&#xa;      SUBSTRING_INDEX&#x28;&#xd;&#xa;        SUBSTRING_INDEX&#x28;DATA, &#x27;finalScore&#x22;&#x3a;&#x27;, - 1&#x29;,&#xd;&#xa;        &#x27;,&#x27;,&#xd;&#xa;        1&#xd;&#xa;      &#x29; final_score &#xd;&#xa;    FROM&#xd;&#xa;      &#x60;finance_order&#x60;.&#x60;ORD_USER_HONOR&#x60; &#xd;&#xa;    WHERE honor_type &#x3d; &#x27;TONGDUN&#x27; &#xd;&#xa;      AND DATA LIKE &#x27;&#x25;finalscore&#x25;&#x27;&#x29; ouh &#xd;&#xa;    ON oo.id &#x3d; ouh.order_id &#xd;&#xa;  LEFT JOIN finance_shop.SOP_SHOP ss &#xd;&#xa;    ON ss.id &#x3d; oo.shop_id &#xd;&#xa;  LEFT JOIN finance_system.SYS_DIC_ITEM sdi &#xd;&#xa;    ON sdi.dic_id &#x3d; 14 &#xd;&#xa;    AND ss.company_type &#x3d; sdi.dic_item_value &#xd;&#xa;  LEFT JOIN &#xd;&#xa;    &#x28;SELECT &#xd;&#xa;      T.order_id,&#xd;&#xa;      GROUP_CONCAT&#x28;&#xd;&#xa;        DISTINCT &#xd;&#xa;        CASE&#xd;&#xa;          WHEN T.c &#x3d; &#x27;SR001&#x27; &#xd;&#xa;          THEN &#x27;&#x8054;&#x7cfb;&#x4eba;&#x53f7;&#x7801;&#x65e0;&#x6548;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SR013&#x27; &#xd;&#xa;          THEN &#x27;&#x90ae;&#x7bb1;&#x6709;&#x8bef;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SR014&#x27; &#xd;&#xa;          THEN &#x27;&#x5355;&#x4f4d;&#x4fe1;&#x606f;&#x9519;&#x8bef;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SR021&#x27; &#xd;&#xa;          THEN &#x27;&#x5730;&#x5740;&#x51fa;&#x9519;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SR031&#x27; &#xd;&#xa;          THEN &#x27;&#x8eab;&#x4efd;&#x8bc1;&#x6a21;&#x7cca;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SR032&#x27; &#xd;&#xa;          THEN &#x27;&#x624b;&#x6301;&#x8eab;&#x4efd;&#x8bc1;&#x81ea;&#x62cd;&#x7167;&#x6a21;&#x7cca;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SR033&#x27; &#xd;&#xa;          THEN &#x27;&#x5408;&#x540c;&#x6b63;&#x9762;&#x7167;&#x7247;&#x6a21;&#x7cca;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SR034&#x27; &#xd;&#xa;          THEN &#x27;&#x8f85;&#x52a9;&#x8bc1;&#x4ef6;&#x6b63;&#x9762;&#x7167;&#x7247;&#x6a21;&#x7cca;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SR041&#x27; &#xd;&#xa;          THEN &#x27;&#x805a;&#x4fe1;&#x7acb;&#x518d;&#x83b7;&#x53d6;_&#x4eac;&#x4e1c;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SR042&#x27; &#xd;&#xa;          THEN &#x27;&#x805a;&#x4fe1;&#x7acb;&#x518d;&#x83b7;&#x53d6;_&#x6dd8;&#x5b9d;&#x27; &#xd;&#xa;        END&#xd;&#xa;      &#x29; AS &#x27;RETURN_REMARK&#x27;,&#xd;&#xa;       GROUP_CONCAT&#x28;&#xd;&#xa;        DISTINCT &#xd;&#xa;        CASE&#xd;&#xa;          WHEN T.c &#x3d; &#x27;SD01&#x27; &#xd;&#xa;          THEN &#x27;&#x8bc1;&#x4ef6;&#x9020;&#x5047;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SD02&#x27; &#xd;&#xa;          THEN &#x27;&#x829d;&#x9ebb;&#x4fe1;&#x7528;&#x5206;&#x4e0d;&#x8db3;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SD03&#x27; &#xd;&#xa;          THEN &#x27;&#x501f;&#x6b3e;&#x4eba;&#x5173;&#x8054;&#x4eba;&#x4fe1;&#x7528;&#x95ee;&#x9898;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SD04&#x27; &#xd;&#xa;          THEN &#x27;&#x4e0d;&#x662f;&#x672c;&#x4eba;&#x6d88;&#x8d39;&#x501f;&#x6b3e;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SD05&#x27; &#xd;&#xa;          THEN &#x27;&#x7f51;&#x8d37;&#x9ed1;&#x540d;&#x5355;&#x7528;&#x6237;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SD06&#x27; &#xd;&#xa;          THEN &#x27;&#x5176;&#x4ed6;&#x5f02;&#x5e38;&#x60c5;&#x51b5;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SD07&#x27; &#xd;&#xa;          THEN &#x27;&#x5e74;&#x9f84;&#x4e0d;&#x7b26;&#x5408;&#x51c6;&#x5165;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SD08&#x27; &#xd;&#xa;          THEN &#x27;&#x4e3b;&#x8d37;&#x4eba;&#x624b;&#x673a;&#x53f7;&#x7801;&#x5f02;&#x5e38;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SD09&#x27; &#xd;&#xa;          THEN &#x27;&#x8eab;&#x4efd;&#x4fe1;&#x606f;&#x9020;&#x5047;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SD10&#x27; &#xd;&#xa;          THEN &#x27;&#x8eab;&#x4efd;&#x76d7;&#x7528;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SD11&#x27; &#xd;&#xa;          THEN &#x27;&#x591a;&#x5934;&#x501f;&#x8d37;&#x60c5;&#x51b5;&#x4e25;&#x91cd;&#x27; &#xd;&#xa;          WHEN T.c &#x3d; &#x27;SD12&#x27; &#xd;&#xa;          THEN &#x27;&#x4e3b;&#x8d37;&#x4eba;&#x7efc;&#x5408;&#x4fe1;&#x7528;&#x4e0d;&#x8db3;&#x27; &#xd;&#xa;        END&#xd;&#xa;      &#x29; AS &#x27;REFUSE_REASON&#x27; &#xd;&#xa;    FROM&#xd;&#xa;      &#x28;SELECT &#xd;&#xa;        a.order_id,&#xd;&#xa;        SUBSTRING_INDEX&#x28;&#xd;&#xa;          SUBSTRING_INDEX&#x28;a.reason, &#x27;,&#x27;, b.help_topic_id &#x2b; 1&#x29;,&#xd;&#xa;          &#x27;,&#x27;,&#xd;&#xa;          - 1&#xd;&#xa;        &#x29; AS c &#xd;&#xa;      FROM&#xd;&#xa;        &#x28;SELECT &#xd;&#xa;          &#x60;order_id&#x60;,&#xd;&#xa;          reason &#xd;&#xa;        FROM&#xd;&#xa;          &#x60;finance_order&#x60;.&#x60;ORD_ORDER_CHECK_HISTORY&#x60; &#xd;&#xa;        WHERE reason LIKE &#x27;&#x25;SR&#x25;&#x27; OR reason LIKE &#x27;&#x25;SD&#x25;&#x27;&#x29; a &#xd;&#xa;        JOIN mysql.help_topic b &#xd;&#xa;          ON b.help_topic_id &#x3c; &#x28;&#xd;&#xa;            LENGTH&#x28;a.reason&#x29; - LENGTH&#x28;REPLACE&#x28;a.reason, &#x27;,&#x27;, &#x27;&#x27;&#x29;&#x29; &#x2b; 1&#xd;&#xa;          &#x29; &#xd;&#xa;      ORDER BY a.order_id&#x29; T &#xd;&#xa;    WHERE T.c &#x3c;&#x3e; &#x27;&#x27; &#xd;&#xa;    GROUP BY 1&#x29; re_table &#xd;&#xa;    ON oo.id &#x3d; re_table.order_id &#xd;&#xa;  </sql>
