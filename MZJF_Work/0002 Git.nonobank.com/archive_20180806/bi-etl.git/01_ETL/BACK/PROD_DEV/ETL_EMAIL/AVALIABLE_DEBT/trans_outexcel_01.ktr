    <sql>select &#x28;CASE WHEN fp.expect_unit &#x3d; 0 THEN add_months&#x28;fp.start_date, fp.expect&#x29;&#xa;      ELSE DATE_ADD&#x28;fp.start_date,fp.expect&#x29;  END&#x29; as day&#xa;      ,sum&#x28;dea.price-dea.lock_price&#x29; as price --&#x53ef;&#x4ee5;&#x91d1;&#x989d;&#xa;from odsopr.debt_exchange_account dea &#xa;  INNER JOIN odsopr.vip_account va &#xa;  on dea.va_id &#x3d; va.id AND dea.bank_flag &#x3d; 1 --&#x5b58;&#x7ba1;&#xa;  INNER JOIN odsopr.borrows bo on dea.bo_id &#x3d; bo.id&#xa;  inner join odsopr.finance_plan fp --&#x5c06;&#x8981;&#x5230;&#x671f;&#x7684;&#x7cbe;&#x9009;&#x4ea7;&#x54c1;&#xa;  on va.fp_id &#x3d;fp.id&#xa;  and fp.scope IN &#x28;0, 1, 11, 13&#x29; --&#x7cbe;&#x9009;&#x3001;&#x65b0;&#x5ba2;&#x3001;&#x8d34;&#x5fc3;&#x3001;0&#x5143;&#xa;  and fp.status&#x3d;1 --&#x7cbe;&#x9009;&#x4ea7;&#x54c1;&#x662f;&#x5df2;&#x7ed3;&#x675f;&#x7684;&#xa;  left outer join  &#x28;select id,asset_side from odsopr.invt_finance_plan_debt_config order by id desc limit 1&#x29; fpdc&#xa;  left outer join odsopr.invt_overdue_bo_tmp tmp --&#x4e0d;&#x826f;&#x8d44;&#x4ea7;&#xa;  on dea.bo_id &#x3d; tmp.bo_id&#xa;  left outer join odsopr.invt_xinke_debt_package xk --&#x503a;&#x6743;&#x672a;&#x6253;&#x5305;&#xa;  on xk.va_id &#x3d; dea.va_id --&#x7cbe;&#x9009;&#x8d26;&#x53f7;&#x91cc;&#x7684;&#x503a;&#x6743;&#xa;  AND xk.bo_id &#x3d; dea.bo_id &#xa;  AND nvl&#x28;xk.seri_no,0&#x29; &#x3d; nvl&#x28;dea.seri_no,0&#x29; --&#xa;  AND xk.is_enable &#x3d; 1&#xa;  left outer join odsopr.invt_intimate_debt_package tx --&#x8d34;&#x5fc3;&#x672a;&#x6253;&#x5305;&#xa;  on tx.va_id &#x3d; dea.va_id --&#x7cbe;&#x9009;&#x8d26;&#x53f7;&#x91cc;&#x7684;&#x503a;&#x6743;&#xa;  AND tx.bo_id &#x3d; dea.bo_id &#xa;  AND nvl&#x28;tx.seri_no,0&#x29; &#x3d; nvl&#x28;dea.seri_no,0&#x29;&#xa;  AND tx.is_enable &#x3d; 1&#xa;  left outer join odsopr.invt_stage_debt_package yys --&#x6708;&#x6708;&#x5347;&#xa;   on yys.va_id &#x3d; dea.va_id --&#x7cbe;&#x9009;&#x8d26;&#x53f7;&#x91cc;&#x7684;&#x503a;&#x6743;&#xa;  AND yys.bo_id &#x3d; dea.bo_id &#xa;  AND nvl&#x28;yys.seri_no,0&#x29; &#x3d; nvl&#x28;dea.seri_no,0&#x29;&#xa;  AND yys.is_enable &#x3d; 1&#xa;where fp.support_stage_pack&#x3c;&#x3e;1 --&#x652f;&#x6301;&#x6253;&#x5305;&#x7684;&#x4ea7;&#x54c1;&#xa;AND fp.id &#x3c;&#x3e; 140 --V3&#xa;and &#x28;CASE WHEN fp.expect_unit &#x3d; 0 THEN add_months&#x28;fp.start_date, fp.expect&#x29;&#xa;      ELSE DATE_ADD&#x28;fp.start_date,fp.expect&#x29;  END&#x29;  between  DATE_ADD&#x28;&#x27;&#x24;&#x7b;STAT_DATE&#x7d;&#x27;,4&#x29; and DATE_ADD&#x28;&#x27;&#x24;&#x7b;STAT_DATE&#x7d;&#x27;,19&#x29; --T&#x2b;4,T&#x2b;15&#x2b;4 8&#x6708;4&#x65e5;&#x53ef;&#x7528;&#x503a;&#x6743; --&#x6253;&#x5305;&#x4ea7;&#x54c1;&#x5230;&#x671f;&#x65e5;&#x671f;&#xa;AND dea.status&#x3e;0 --&#x975e;&#x521d;&#x59cb;&#x5316;&#x72b6;&#x6001;&#xa;and dea.hold_num-dea.locking_num&#x3e;0&#xa;and array_contains&#x28;split&#x28;fpdc.asset_side,&#x27;,&#x27;&#x29;,cast&#x28;bo.p_id as string&#x29;&#x29; --&#x7cbe;&#x9009;&#x4ea7;&#x54c1;&#x53ef;&#x6253;&#x5305;&#x7684;pid&#xa;and tmp.bo_id is null --&#x4e0d;&#x826f;&#x8d44;&#x4ea7;&#xa;and xk.bo_id is null&#xa;and tx.bo_id is null&#xa;and yys.bo_id is null&#xa;group by &#x28;CASE WHEN fp.expect_unit &#x3d; 0 THEN add_months&#x28;fp.start_date, fp.expect&#x29; ELSE DATE_ADD&#x28;fp.start_date,fp.expect&#x29;  END&#x29;</sql>
