    <sql>SELECT  CONCAT&#x28;A,B,C,D,E&#x29;  AS T_ES_EXC_SQL&#xd;&#xa;FROM &#x28;&#xd;&#xa;SELECT &#x27;SELECT &#x27; A &#xd;&#xa;, CASE WHEN LENGTH&#x28;IFNULL&#x28;&#x27;&#x24;&#x7b;FIELD_LIST&#x7d;&#x27;,&#x27;&#x27;&#x29;&#x29;&#x3e;0 THEN &#x27;&#x24;&#x7b;FIELD_LIST&#x7d;&#x27; ELSE &#x27;&#x2a;&#x27; END B&#xd;&#xa;, &#x27; FROM &#x27; C&#xd;&#xa;,&#x27;&#x24;&#x7b;FROM_TABLE_SCHEMA&#x7d;.&#x24;&#x7b;FROM_TABLE_NAME&#x7d;&#x27; D&#xd;&#xa;,CASE WHEN &#x27;&#x24;&#x7b;IS_INCREMENT_RANGE&#x7d;&#x27;&#x3d;1 AND &#x27;&#x24;&#x7b;TASK_ETL_TYPE&#x7d;&#x27;&#x3d;&#x27;I&#x27; AND &#x27;&#x24;&#x7b;INCREMENT_TYPE&#x7d;&#x27;&#x3d;1 AND LENGTH&#x28;IFNULL&#x28;&#x27;&#x24;&#x7b;START_VALUE&#x7d;&#x27;,&#x27;&#x27;&#x29;&#x29;&#x3e;0 THEN &#x27; WHERE &#x24;&#x7b;V_INCREMENT_FIELD&#x7d; &#x3e;&#x3d; &#x27;&#x27;&#x24;&#x7b;START_VALUE&#x7d;&#x27;&#x27; AND &#x24;&#x7b;V_INCREMENT_FIELD&#x7d; &#x3c; &#x27;&#x27;&#x24;&#x7b;V_END_VALUE&#x7d;&#x27;&#x27;&#x27;&#xd;&#xa;      WHEN &#x27;&#x24;&#x7b;IS_INCREMENT_RANGE&#x7d;&#x27;&#x3d;1 AND &#x27;&#x24;&#x7b;TASK_ETL_TYPE&#x7d;&#x27;&#x3d;&#x27;I&#x27; AND &#x27;&#x24;&#x7b;INCREMENT_TYPE&#x7d;&#x27;&#x3d;2 AND LENGTH&#x28;IFNULL&#x28;&#x27;&#x24;&#x7b;START_VALUE&#x7d;&#x27;,&#x27;&#x27;&#x29;&#x29;&#x3e;0 THEN &#x27; WHERE &#x24;&#x7b;V_INCREMENT_FIELD&#x7d; &#x3e;&#x3d; &#x24;&#x7b;START_VALUE&#x7d; AND &#x24;&#x7b;V_INCREMENT_FIELD&#x7d; &#x3c; &#x24;&#x7b;V_END_VALUE&#x7d;&#x27;&#xd;&#xa;      WHEN &#x27;&#x24;&#x7b;IS_INCREMENT_RANGE&#x7d;&#x27;&#x3d;0 AND &#x27;&#x24;&#x7b;TASK_ETL_TYPE&#x7d;&#x27;&#x3d;&#x27;I&#x27; THEN &#x27; WHERE &#x24;&#x7b;INCREMENT_FIELD&#x7d; &#x3e;&#x3d; &#x27;&#x27;&#x24;&#x7b;LAST_DATA_TIME&#x7d;&#x27;&#x27; &#x27; ELSE &#x27;   &#x27; END E&#xd;&#xa;,&#x27; &#x3b;&#x27;F&#xd;&#xa;&#x29; AS T&#xd;&#xa;&#x3b;</sql>
