    <sql>&#x2f;&#x2a;&#x4e34;&#x65f6;&#x8868;&#x6784;&#x5efa;&#x2a;&#x2f;&#xd;&#xa;&#xd;&#xa;INSERT overwrite table dmt_im.mz_day_income_new_log_V2&#xd;&#xa;SELECT &#x2a; FROM dmt_im.mz_day_income_new_log_V2 &#xd;&#xa;WHERE create_time&#x3c;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- &#x8ffd;&#x52a0;&#x4fdd;&#x7406;&#x8d39;&#x7528;&#xd;&#xa;INSERT INTO dmt_im.mz_day_income_new_log_V2&#xd;&#xa;SELECT &#xd;&#xa;&#x22;&#x9ea6;&#x82bd;&#x22; busi_line&#xd;&#xa;,fl.account_id&#xd;&#xa;,fl.role_id&#xd;&#xa;,to_date&#x28;fl.create_time&#x29; v_date&#xd;&#xa;,fl.operation_relation&#xd;&#xa;,ROUND&#x28;SUM&#x28;fl.income&#x29;,2&#x29; income&#xd;&#xa;,ROUND&#x28;SUM&#x28;fl.expend&#x29;,2&#x29; expend&#xd;&#xa;,COUNT&#x28;1&#x29; cnt&#xd;&#xa;,CAST&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS STRING&#x29; create_time&#xd;&#xa;FROM &#xd;&#xa;-- odsopr.finance_log_all fl&#xd;&#xa;ods.t_finance_log_hist fl&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;AND account_id&#x3d;1086356&#xd;&#xa;AND operation_relation&#x3d;571&#xd;&#xa;AND fl.create_time&#x3e;&#x3d;&#x22;2017-06-01&#x22;&#xd;&#xa;AND fl.create_time&#x3e;&#x3d;DATE_SUB&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;,7&#x29;&#xd;&#xa;AND fl.create_time&#x3c;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;GROUP BY fl.account_id,fl.role_id,to_date&#x28;fl.create_time&#x29; ,fl.operation_relation &#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- &#x4e1a;&#x52a1;&#x6263;&#x9664;&#x54a8;&#x8be2;&#x8d39; -2 &#x903e;&#x671f;&#x6263;&#x9664; -3 &#x64a4;&#x9500;&#x6263;&#x9664;&#xd;&#xa;INSERT INTO dmt_im.mz_day_income_new_log_V2&#xd;&#xa;SELECT &#xd;&#xa;bo.busi_line&#xd;&#xa;,-2 account_id&#xd;&#xa;,-2 role_id&#xd;&#xa;,to_date&#x28;bcfl.update_time&#x29; v_date&#xd;&#xa;,IF&#x28;bcfl.txn_type&#x3d;1,-2,-3&#x29; operation_relation&#xd;&#xa;,ROUND&#x28;SUM&#x28;IF&#x28;bcfl.txn_type&#x3d;1,before_amount,0&#x29;&#x29;,2&#x29; income&#xd;&#xa;,ROUND&#x28;SUM&#x28;IF&#x28;bcfl.txn_type&#x3d;1,0,before_amount&#x29;&#x29;,2&#x29;  expend&#xd;&#xa;,COUNT&#x28;1&#x29; cnt&#xd;&#xa;,CAST&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29; AS STRING&#x29; create_time&#xd;&#xa;FROM odsopr.borrows_consulted_fee_log  bcfl &#xd;&#xa;-- JOIN dmt_im.v_borrows_busiline_product_bibi3 bo ON bo.bo_id&#x3d;bcfl.bo_id&#xd;&#xa;LEFT JOIN dmt_im.v_borrows_busiline_product_bibi3 bo ON bo.bo_id&#x3d;bcfl.bo_id&#xd;&#xa;WHERE bcfl.txn_type  IN&#x28;1,3&#x29; &#xd;&#xa;AND bcfl.create_time &#x3e;&#x3d; DATE_SUB&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;,7&#x29; &#xd;&#xa;AND bcfl.create_time &#x3e;&#x3d; &#x22;2017-06-05&#x22; &#xd;&#xa;AND bcfl.create_time &#x3c; to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;AND bcfl.status &#x3d; 1 &#xd;&#xa;GROUP BY bo.busi_line,to_date&#x28;bcfl.update_time&#x29;,IF&#x28;bcfl.txn_type&#x3d;1,-2,-3&#x29;&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;-- &#x54a8;&#x8be2;&#x8d39;&#x6263;&#x9664;&#xd;&#xa;INSERT INTO dmt_im.mz_day_income_new_log_V2&#xd;&#xa;SELECT &#xd;&#xa;bo.busi_line&#xd;&#xa;,-2 account_id&#xd;&#xa;,-2 role_id&#xd;&#xa;,to_date&#x28;bcf.update_time&#x29; v_date&#xd;&#xa;,-2 operation_relation&#xd;&#xa;,ROUND&#x28;SUM&#x28;bcf.zx_price&#x29;,2&#x29; income&#xd;&#xa;,0 expend&#xd;&#xa;,COUNT&#x28;1&#x29; cnt&#xd;&#xa;,CAST&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29; AS STRING&#x29; create_time&#xd;&#xa;&#xd;&#xa;FROM odsopr.borrows_consulted_fee bcf&#xd;&#xa;-- JOIN dmt_im.v_borrows_busiline_product_bibi3 bo ON bo.bo_id&#x3d;bcf.bo_id&#xd;&#xa;LEFT JOIN dmt_im.v_borrows_busiline_product_bibi3 bo ON bo.bo_id&#x3d;bcf.bo_id&#xd;&#xa;WHERE bcf.zx_state &#x3d; 2 &#xd;&#xa;AND bcf.update_time &#x3e;&#x3d;DATE_SUB&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;,7&#x29; &#xd;&#xa;AND bcf.update_time &#x3e;&#x3d;&#x22;2017-06-05&#x22; &#xd;&#xa;AND bcf.update_time &#x3c;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;GROUP BY bo.busi_line,to_date&#x28;bcf.update_time&#x29;&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;-- &#x8d22;&#x795e;&#x4e1a;&#x52a1;&#x6570;&#x636e;&#x8865;&#x5165;&#xd;&#xa;INSERT INTO dmt_im.mz_day_income_new_log_V2&#xd;&#xa;SELECT &#xd;&#xa;&#x22;&#x8d22;&#x795e;&#x7237;&#x7237;&#x22; busi_line&#xd;&#xa;,-3 account_id&#xd;&#xa;,-3 role_id&#xd;&#xa;,SUBSTR&#x28;up.created_date,1,10&#x29; v_date&#xd;&#xa;,-4 operation_relation&#xd;&#xa;,0 income&#xd;&#xa;,ROUND&#x28;SUM&#x28;invest_amount&#x29;,2&#x29;  expend&#xd;&#xa;,COUNT&#x28;1&#x29; cnt&#xd;&#xa;,CAST&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29; AS STRING&#x29; create_time&#xd;&#xa;FROM odsopr.user_product up&#xd;&#xa;WHERE STATUS&#x3d;&#x22;A&#x22;&#xd;&#xa;AND up.created_date&#x3e;&#x3d; DATE_SUB&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;,7&#x29;&#xd;&#xa;AND up.created_date&#x3c;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;GROUP BY SUBSTR&#x28;up.created_date,1,10&#x29;&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO dmt_im.mz_day_income_new_log_V2&#xd;&#xa;SELECT &#xd;&#xa;&#x22;&#x8d22;&#x795e;&#x7237;&#x7237;&#x22; busi_line&#xd;&#xa;,-3 account_id&#xd;&#xa;,-3 role_id&#xd;&#xa;,SUBSTR&#x28;wpc.card_update_date,1,10&#x29; v_date&#xd;&#xa;,-5 operation_relation&#xd;&#xa;,0 income&#xd;&#xa;,ROUND&#x28;SUM&#x28;privilege_amount&#x29;,2&#x29;  expend&#xd;&#xa;,COUNT&#x28;1&#x29; cnt&#xd;&#xa;,CAST&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29; AS STRING&#x29; create_time&#xd;&#xa;FROM odsopr.welfare_privilege_code wpc&#xd;&#xa;WHERE welfare_type&#x3d;5&#xd;&#xa;AND use_status&#x3d;1&#xd;&#xa;AND wpc.card_update_date&#x3e;&#x3d; DATE_SUB&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;,7&#x29;&#xd;&#xa;AND wpc.card_update_date&#x3c;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;GROUP BY SUBSTR&#x28;wpc.card_update_date,1,10&#x29;&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;INSERT INTO dmt_im.mz_day_income_new_log_V2&#xd;&#xa;SELECT &#xd;&#xa;&#x22;&#x8d22;&#x795e;&#x7237;&#x7237;&#x22; busi_line&#xd;&#xa;,-3 account_id&#xd;&#xa;,-3 role_id&#xd;&#xa;,SUBSTR&#x28;ui.created_date,1,10&#x29; v_date&#xd;&#xa;,-6 operation_relation&#xd;&#xa;,0 income&#xd;&#xa;,ROUND&#x28;SUM&#x28;reward_interest&#x29;,2&#x29;  expend&#xd;&#xa;,COUNT&#x28;1&#x29; cnt&#xd;&#xa;,CAST&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29; AS STRING&#x29; create_time&#xd;&#xa;FROM odsopr.user_interest ui&#xd;&#xa;WHERE 1&#x3d;1 AND reward_interest&#x3e;0&#xd;&#xa;AND ui.created_date&#x3e;&#x3d; DATE_SUB&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;,7&#x29;&#xd;&#xa;AND ui.created_date&#x3c;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;GROUP BY SUBSTR&#x28;ui.created_date,1,10&#x29;&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--  &#x5fc5;&#x987b;hive&#xd;&#xa;INSERT INTO dmt_im.mz_day_income_new_log_V2&#xd;&#xa;SELECT &#xd;&#xa;&#x22;&#x8d22;&#x795e;&#x7237;&#x7237;&#x22; busi_line&#xd;&#xa;,-3 account_id&#xd;&#xa;,-3 role_id&#xd;&#xa;,SUBSTR&#x28;a.create_date,1,10&#x29; v_date&#xd;&#xa;,-7 operation_relation&#xd;&#xa;,ROUND&#x28;SUM&#x28;IF&#x28;kind&#x3d;3,sxf,0&#x29;&#x29;,2&#x29;  income&#xd;&#xa;,ROUND&#x28;SUM&#x28;IF&#x28;kind&#x3d;3,0,sxf&#x29;&#x29;,2&#x29;  expend&#xd;&#xa;,COUNT&#x28;1&#x29; cnt&#xd;&#xa;,CAST&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29; AS STRING&#x29; create_time&#xd;&#xa;&#xd;&#xa;FROM &#xd;&#xa;&#x28;&#xd;&#xa;SELECT a.user_code,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;trans_date,&#x22;yyyyMMdd&#x22;&#x29;,&#x22;yyyy-MM-dd&#x22;&#x29;&#xd;&#xa;create_date,a.trans_code,a.amount&#xd;&#xa;,CASE WHEN a.trans_date&#x3c;&#x22;20150929&#x22; THEN a.amount&#x2a;0.003 &#xd;&#xa;WHEN a.trans_date&#x3c;&#x22;20160101&#x22; THEN a.amount&#x2a;0.002 &#xd;&#xa;ELSE a.amount&#x2a;0.0012 END sxf&#xd;&#xa;,1 kind&#xd;&#xa;FROM odsopr.trans_detail a&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;AND trans_date&#x3e;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;DATE_SUB&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;,7&#x29;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;AND trans_date&#x3c;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;,&#x22;yyyy-MM-dd&#x22;&#x29;,&#x22;yyyyMMdd&#x22;&#x29;&#xd;&#xa;AND a.trans_code IN&#x28;0,1&#x29;&#xd;&#xa;AND a.user_code &#x21;&#x3d;999999999 &#xd;&#xa;AND a.status&#x3d;&#x22;2&#x22;&#xd;&#xa;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT a.user_code,a.create_date,a.trans_code,-a.withdraw_amount as amount&#xd;&#xa;,CASE WHEN a.create_date&#x3c;&#x22;2015-09-29&#x22; THEN 1.2 ELSE 1.0 END sxf&#xd;&#xa;,2 kind&#xd;&#xa;FROM odsopr.withdraw_reward a&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;AND create_date&#x3e;&#x3d; DATE_SUB&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;,7&#x29;&#xd;&#xa;AND create_date&#x3c;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;AND a.user_code &#x21;&#x3d;999999999 &#xd;&#xa;AND a.status IN&#x28;2,3,8&#x29; &#xd;&#xa;&#xd;&#xa;UNION ALL&#xd;&#xa;&#xd;&#xa;SELECT wr.user_code,wr.create_date,wr.trans_code,0 as amount,wml.less_amount as sxf,3 kind&#xd;&#xa;FROM odsopr.user_withdraw_money_log wml&#xd;&#xa;LEFT JOIN odsopr.withdraw_reward wr ON wr.orderid&#x3d; wml.str1&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;AND wr.create_date&#x3e;&#x3d; DATE_SUB&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;,7&#x29;&#xd;&#xa;AND wr.create_date&#x3c;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;AND wr.user_code &#x21;&#x3d;999999999 &#xd;&#xa;&#x29; a&#xd;&#xa;GROUP BY SUBSTR&#x28;a.create_date,1,10&#x29;&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- -8 &#x73b0;&#x91d1;&#x5956;&#x52b1;&#xd;&#xa;INSERT INTO dmt_im.mz_day_income_new_log_V2&#xd;&#xa;SELECT &#xd;&#xa;&#x22;&#x8d22;&#x795e;&#x7237;&#x7237;&#x22; busi_line&#xd;&#xa;,-3 account_id&#xd;&#xa;,-3 role_id&#xd;&#xa;,SUBSTR&#x28;a.create_date,1,10&#x29; v_date&#xd;&#xa;,-8 operation_relation&#xd;&#xa;,0  income&#xd;&#xa;,ROUND&#x28;SUM&#x28;amount&#x29;,2&#x29;  expend&#xd;&#xa;,COUNT&#x28;1&#x29; cnt&#xd;&#xa;,CAST&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29; AS STRING&#x29; create_time&#xd;&#xa;FROM odsopr.user_account_trans_log a&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;AND operation_type &#x3d;3&#xd;&#xa;AND create_date&#x3e;&#x3d; DATE_SUB&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;,7&#x29;&#xd;&#xa;AND create_date&#x3c;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;GROUP BY SUBSTR&#x28;a.create_date,1,10&#x29;&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- unifi &#x6b63;&#x5e38;&#x8fd8;&#x6b3e;&#x670d;&#x52a1;&#x8d39;&#xd;&#xa;&#xd;&#xa;INSERT INTO dmt_im.mz_day_income_new_log_V2&#xd;&#xa;SELECT &#xd;&#xa;&#x22;Unif&#x22; busi_line&#xd;&#xa;,-1 account_id&#xd;&#xa;,-1 role_id&#xd;&#xa;,to_date&#x28;br.br_repay_time&#x29; v_date&#xd;&#xa;,-1 operation_relation&#xd;&#xa;,ROUND&#x28;SUM&#x28;br.br_service_fee&#x2b;br.br_repayed_tech_fee&#x29;,2&#x29; income&#xd;&#xa;,0 expend&#xd;&#xa;,COUNT&#x28;1&#x29; cnt&#xd;&#xa;,CAST&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29; AS STRING&#x29; create_time&#xd;&#xa;FROM  odsopr.borrows_repayment br&#xd;&#xa;-- JOIN dmt_im.v_borrows_busiline_product_bibi3 bo ON bo.bo_id&#x3d;br.bo_id&#xd;&#xa;JOIN dmt_im.v_borrows_busiline_product_bibi3 bo ON bo.bo_id&#x3d;br.bo_id&#xd;&#xa;WHERE 1&#x3d;1 AND br.br_repay_time&#x3e;&#x3d;&#x22;2017-06-05&#x22;&#xd;&#xa;&#xd;&#xa;AND br.br_repay_time&#x3e;&#x3d;DATE_SUB&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;,7&#x29;&#xd;&#xa;AND br.br_repay_time&#x3c;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;AND br.br_overdue_terms&#x3d;0&#xd;&#xa;AND bo.busi_line IN&#x28;&#x22;Unifi&#x22;&#x29;&#xd;&#xa;GROUP BY to_date&#x28;br.br_repay_time&#x29;&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;-- &#x5e73;&#x53f0;&#x6536;&#x5165;&#x3001;&#x652f;&#x51fa;&#x3001;&#x5f52;&#x8fd8;&#x4ee3;&#x507f;&#x6536;&#x5165;&#x3001;&#x5f52;&#x8fd8;&#x4ee3;&#x507f;&#x652f;&#x51fa;&#x8d26;&#x6237; &#x6536;&#x5165;&#x76f8;&#x5173;&#x64cd;&#x4f5c;&#xd;&#xa;-- &#x6839;&#x636e;&#x8d22;&#x52a1;&#x6c9f;&#x901a;&#xff0c;&#x90a3;&#x4e9b;&#x9879;&#x76ee;&#x8ba1;&#x7b97;&#x6536;&#x5165;&#x52a0;&#x5165;&#x7edf;&#x8ba1;&#xd;&#xa;-- &#x8ddf;&#x8fdb;&#x8d44;&#x91d1;&#x5907;&#x6ce8;&#xff0c;&#x5212;&#x5206;&#x4e1a;&#x52a1;&#x7ebf;&#x5f52;&#x5c5e;&#xd;&#xa;-- &#x5982;&#x679c;&#x6709;&#x6807;&#x7684;&#x53f7;&#xff0c;&#x6309;&#x7167;&#x6807;&#x7684;&#x53f7;&#x5212;&#x5206;&#xff0c;&#x5982;&#x679c;&#x6ca1;&#x6709;&#x6309;&#x7167;&#x4eba;&#x7684;&#x8eab;&#x4efd;&#x5212;&#x5206;&#xff1b;&#xd;&#xa;&#xd;&#xa;INSERT INTO dmt_im.mz_day_income_new_log_V2&#xd;&#xa;seLECT &#xd;&#xa;CASE WHEN fl.operation_relation&#x3d;698 AND fl.remark LIKE &#x22;&#x25;&#x5df2;&#x8fbe;&#x6700;&#x5927;&#x6536;&#x76ca;&#x25;&#x22; THEN &#x22;&#x8bfa;&#x5b9c;&#x8d22;&#x5bcc;&#x22;&#xd;&#xa;WHEN fl.operation_relation in &#x28;694,696&#x29; THEN &#x22;&#x8bfa;&#x5b9c;&#x8d22;&#x5bcc;&#x22;&#xd;&#xa;WHEN fl.operation_relation&#x3d;714 THEN &#x22;&#x9ea6;&#x82bd;&#x22;&#xd;&#xa;WHEN fl.operation_relation&#x3d;711 THEN &#x22;&#x878d;&#x6613;&#x8d37;&#x22;&#xd;&#xa;&#xd;&#xa;WHEN fl.operation_relation IN&#x28;681,683,692,696,731,698,708,732,745&#x29; THEN &#xd;&#xa;&#x28;CASE WHEN fl.operation_relation&#x21;&#x3d;708 and ui2.register_biz&#x3d;&#x22;csyy&#x22; THEN &#x22;&#x8d22;&#x795e;&#x7237;&#x7237;&#x22; &#xd;&#xa;WHEN fl.operation_relation&#x21;&#x3d;708 and ui2.register_biz&#x3d;&#x22;nono&#x22; THEN &#x22;&#x8bfa;&#x5b9c;&#x8d22;&#x5bcc;&#x22;&#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;mxd&#x22; and substr&#x28;ui2.edu_graduate_date,1,4&#x29;&#x3e;&#x27;2017&#x27; AND ui2.edu_graduate_date is not null  THEN &#x22;&#x5b66;&#x751f;&#x8d37;&#x22;&#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;mxd&#x22;  THEN &#x22;&#x4fe1;&#x7528;&#x8d37;&#x22;&#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;dfd&#x22; THEN &#x22;&#x5927;&#x623f;&#x4e1c;&#x22; &#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;maiya&#x22; THEN &#x22;&#x9ea6;&#x82bd;&#x22; &#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;mld&#x22; THEN &#x22;&#x9ea6;&#x7c92;&#x8d37;&#x22;&#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;unifi&#x22; THEN &#x22;Unif&#x22;&#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;bld&#x22; THEN &#x22;&#x767d;&#x9886;&#x8d37;&#x22; &#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;mzjk&#x22; THEN &#x22;&#x9ea6;&#x5b50;&#x501f;&#x6b3e;&#x22; &#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;cdd&#x22; THEN &#x22;&#x5927;&#x623f;&#x4e1c;&#x22; ELSE &#x22;&#x878d;&#x6613;&#x8d37;&#x22; end&#xd;&#xa;&#xd;&#xa;&#x29;&#xd;&#xa;when bo2.busi_line is not null then bo2.busi_line&#xd;&#xa;ELSE &#x28;CASE &#xd;&#xa;-- WHEN ui2.register_biz&#x3d;&#x22;csyy&#x22; THEN &#x22;&#x8d22;&#x795e;&#x7237;&#x7237;&#x22; &#xd;&#xa;-- WHEN ui2.register_biz&#x3d;&#x22;nono&#x22; THEN &#x22;&#x8bfa;&#x5b9c;&#x8d22;&#x5bcc;&#x22;&#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;mxd&#x22; and substr&#x28;ui2.edu_graduate_date,1,4&#x29;&#x3e;&#x27;2017&#x27; AND ui2.edu_graduate_date is not null  THEN &#x22;&#x5b66;&#x751f;&#x8d37;&#x22;&#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;mxd&#x22;  THEN &#x22;&#x4fe1;&#x7528;&#x8d37;&#x22;&#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;dfd&#x22; THEN &#x22;&#x5927;&#x623f;&#x4e1c;&#x22; &#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;maiya&#x22; THEN &#x22;&#x9ea6;&#x82bd;&#x22; &#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;mld&#x22; THEN &#x22;&#x9ea6;&#x7c92;&#x8d37;&#x22;&#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;unifi&#x22; THEN &#x22;Unif&#x22;&#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;bld&#x22; THEN &#x22;&#x767d;&#x9886;&#x8d37;&#x22; &#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;mzjk&#x22; THEN &#x22;&#x9ea6;&#x5b50;&#x501f;&#x6b3e;&#x22; &#xd;&#xa;WHEN ui2.register_biz&#x3d;&#x22;cdd&#x22; THEN &#x22;&#x5927;&#x623f;&#x4e1c;&#x22; ELSE &#x22;&#x878d;&#x6613;&#x8d37;&#x22; END&#xd;&#xa;&#x29; END busi_line&#xd;&#xa;,fl.account_id&#xd;&#xa;,fr.id role_id&#xd;&#xa;,to_date&#x28;fl.create_time&#x29; v_date&#xd;&#xa;,fl.operation_relation&#xd;&#xa;,ROUND&#x28;SUM&#x28;fl.income&#x29;,2&#x29; income&#xd;&#xa;,ROUND&#x28;SUM&#x28;fl.expend&#x29;,2&#x29; expend&#xd;&#xa;,COUNT&#x28;1&#x29; cnt&#xd;&#xa;,CAST&#x28;to_date&#x28;from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29; AS STRING&#x29; create_time&#xd;&#xa;FROM odsopr.finance_account fa &#xd;&#xa;JOIN odsopr.finance_role fr ON fr.id&#x3d;fa.role_id&#xd;&#xa;JOIN  ods.t_finance_log_hist fl ON fl.account_id&#x3d;fa.id&#xd;&#xa;-- JOIN  odsopr.finance_log_all fl ON fl.account_id&#x3d;fa.id&#xd;&#xa;LEFT JOIN odsopr.user_info ui ON ui.id&#x3d;fa.user_id&#xd;&#xa;LEFT JOIN odsopr.finance_operation_relation forr ON forr.id&#x3d;fl.operation_relation&#xd;&#xa;LEFT JOIN odsopr.finance_account fa2 ON fa2.id&#x3d;fl.opposite_account_id &#xd;&#xa;LEFT JOIN idw.dim_user ui2 ON ui2.user_id&#x3d;fa2.user_id&#xd;&#xa;LEFT JOIN odsopr.finance_role fr2 ON fr2.id&#x3d;fa2.role_id&#xd;&#xa;LEFT JOIN idw.fact_borrows_repayment br &#xd;&#xa;ON CAST&#x28;br.br_id AS STRING&#x29;&#x3d;regexp_extract&#x28;regexp_replace&#x28;fl.remark,&#x27;,&#x27;,&#x27;&#x27;&#x29;,&#x27;&#x516c;&#x53f8;&#x4ee3;&#x507f;&#x7b2c;&#x28;&#x5b;0-9&#x5d;&#x2b;&#x29;&#x671f;&#x8fd8;&#x6b3e;&#x27;,1&#x29;&#xd;&#xa;&#xd;&#xa;---LEFT JOIN dmt_im.v_borrows_busiline_product_bibi3 bo3 ON bo3.bo_id&#x3d;br.bo.id&#xd;&#xa;LEFT JOIN odsopr.borrows bo ON bo.id&#x3d;CAST&#x28;fl.key_value AS INT&#x29; &#xd;&#xa;AND &#x28;fl.table_name&#x3d;&#x22;borrows&#x22; or fl.operation_relation in &#x28;738,740,742&#x29; &#x29;&#xd;&#xa;&#xd;&#xa; left join odsopr.acc_pay_back_repay_detail rd on rd.pay_no&#x3d;regexp_extract&#x28;regexp_replace&#x28;fl.remark,&#x27;,&#x27;,&#x27;&#x27;&#x29;,&#x27;&#x5f52;&#x8fd8;&#x4ee3;&#x507f;&#x8fd8;&#x6b3e;&#x652f;&#x4ed8;&#x5355;&#x53f7;&#x3a;&#x28;&#x5b;0-9&#x5d;&#x2b;&#x29;&#x27;,1&#x29;&#xd;&#xa;LEFT JOIN odsopr.trans_repayment_order ro ON ro.trans_id&#x3d;rd.trd_no&#xd;&#xa;LEFT JOIN odsopr.borrows_repayment br2 ON br2.id&#x3d;CAST&#x28;ro.repayment_id AS BIGINT&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- LEFT JOIN dmt_im.v_borrows_busiline_product_bibi3 bo2 ON bo2.bo_id&#x3d;IFNULL&#x28;bo.id,br.bo_id&#x29;&#xd;&#xa; LEFT JOIN dmt_im.v_borrows_busiline_product_bibi3 bo2 ON bo2.bo_id&#x3d;IFNULL&#x28;bo.id,IFNULL&#x28;br.bo_id,br2.bo_id&#x29;&#x29;&#xd;&#xa; &#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;&#xd;&#xa;AND fr.id IN&#x28;65,66,68,69&#x29;&#xd;&#xa;-- AND fl.operation_relation IN&#x28;708,711,700,720,731,720,692,714,698,696,692,698,731,681,683,681,722,732&#x29;&#xd;&#xa;AND fl.operation