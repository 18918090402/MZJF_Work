    <sql>LOCK TABLES ETL_CONTROL_RESOURCE_POOL WRITE,ETL_CONTROL_JOB READ,ETL_CONTROL_TASK_DW B READ&#xd;&#xa;,ETL_CONTROL_TASK_LAST_TIME C READ,ETL_CONTROL_TASK_PARENT D READ,ETL_CONTROL_TASK_LAST_TIME E READ,ETL_CONTROL_RESOURCE_POOL P WRITE&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO ETL_CONTROL_RESOURCE_POOL&#x28;RUN_ID,&#xd;&#xa;  JOB_ID,&#xd;&#xa;  JOB_NAME,&#xd;&#xa;  JOB_TYPE,&#xd;&#xa;  TASK_ID,&#xd;&#xa;  TASK_NAME,&#xd;&#xa;  TASK_ETL_TYPE,&#xd;&#xa;  LAST_DATA_TIME,&#xd;&#xa;  LAST_DELETE_TIME,&#xd;&#xa;  LAST_PARENT_DATA_TIME,&#xd;&#xa;  LAST_MERGE_TIME,&#xd;&#xa;  DW_CREATE_TIME &#x29;&#xd;&#xa;SELECT &#xd;&#xa;  RUN_ID,&#xd;&#xa;  JOB_ID,&#xd;&#xa;  JOB_NAME,&#xd;&#xa;  JOB_TYPE,&#xd;&#xa;  TASK_ID,&#xd;&#xa;  TASK_NAME,&#xd;&#xa;  TASK_ETL_TYPE,&#xd;&#xa;  LAST_DATA_TIME,&#xd;&#xa;  LAST_DELETE_TIME,&#xd;&#xa;  LAST_PARENT_DATA_TIME,&#xd;&#xa;  LAST_MERGE_TIME,&#xd;&#xa;  DW_CREATE_TIME &#xd;&#xa;FROM&#xd;&#xa;  &#x28;&#xd;&#xa;  SELECT &#xd;&#xa;  RUN_ID,&#xd;&#xa;  JOB_ID,&#xd;&#xa;  CASE&#xd;&#xa;    WHEN UPPER&#x28;REFRESH_FREQ_NAME&#x29; &#x3d; &#x27;ONCE&#x27; THEN 1 &#xd;&#xa;    WHEN UPPER&#x28;REFRESH_FREQ_NAME&#x29; &#x3d; &#x27;MINUTE&#x27; THEN 2 &#xd;&#xa;    WHEN UPPER&#x28;REFRESH_FREQ_NAME&#x29; &#x3d; &#x27;HOURLY&#x27; THEN 3 &#xd;&#xa;    WHEN UPPER&#x28;REFRESH_FREQ_NAME&#x29; &#x3d; &#x27;DAILY&#x27; THEN 4 &#xd;&#xa;    WHEN UPPER&#x28;REFRESH_FREQ_NAME&#x29; &#x3d; &#x27;WEEKLY&#x27; THEN 5 &#xd;&#xa;    WHEN UPPER&#x28;REFRESH_FREQ_NAME&#x29; &#x3d; &#x27;MONTHLY&#x27; THEN 6 &#xd;&#xa;    WHEN UPPER&#x28;REFRESH_FREQ_NAME&#x29; &#x3d; &#x27;YEARLY&#x27; THEN 7 &#xd;&#xa;    ELSE 99 END REFRESH_FREQ_NAME,&#xd;&#xa;  IFNULL&#x28;REFRESH_FREQ_NUM, 0&#x29; REFRESH_FREQ_NUM,&#xd;&#xa;  JOB_NAME,&#xd;&#xa;  JOB_TYPE,&#xd;&#xa;  TASK_ID,&#xd;&#xa;  TASK_NAME,&#xd;&#xa;  TASK_ETL_TYPE,&#xd;&#xa;  LAST_DATA_TIME,&#xd;&#xa;  LAST_DELETE_TIME,&#xd;&#xa;  LAST_PARENT_DATA_TIME,&#xd;&#xa;  LAST_MERGE_TIME,&#xd;&#xa;  DW_CREATE_TIME,&#xd;&#xa;  LAST_DATA_TIME_ASC,&#xd;&#xa;  TASK_PRI&#xd;&#xa;FROM&#xd;&#xa;  &#x28;SELECT &#xd;&#xa;    123 RUN_ID, -- &#x53d8;&#x91cf;&#x66ff;&#x6362;&#xd;&#xa;    A.ID AS JOB_ID,&#xd;&#xa;    A.JOB_NAME,&#xd;&#xa;    A.JOB_TYPE,&#xd;&#xa;    B.ID AS TASK_ID,&#xd;&#xa;    B.TASK_NAME AS TASK_NAME,&#xd;&#xa;    B.TASK_ETL_TYPE,&#xd;&#xa;    DATE_ADD&#x28;IFNULL&#x28;C.LAST_DATA_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29;,INTERVAL - B.OFFSET MINUTE&#x29; LAST_DATA_TIME,&#xd;&#xa;    IFNULL&#x28;C.LAST_DELETE_TIME, DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29; LAST_DELETE_TIME,&#xd;&#xa;    MIN&#x28;IFNULL&#x28;E.LAST_DATA_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29;&#x29; LAST_PARENT_DATA_TIME,&#xd;&#xa;    IFNULL&#x28;C.LAST_MERGE_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29; LAST_MERGE_TIME,&#xd;&#xa;    SYSDATE&#x28;&#x29; DW_CREATE_TIME,&#xd;&#xa;    IFNULL&#x28;C.LAST_DATA_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29; LAST_DATA_TIME_REAL,&#xd;&#xa;    UPPER&#x28;B.REFRESH_FREQ_NAME&#x29; REFRESH_FREQ_NAME,&#xd;&#xa;    B.REFRESH_FREQ_NUM,&#xd;&#xa;    IFNULL&#x28;C.LAST_DATA_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29; AS LAST_DATA_TIME_ASC,&#xd;&#xa;    B.TASK_PRI&#xd;&#xa;  FROM&#xd;&#xa;    &#x28;SELECT &#xd;&#xa;      &#x2a; &#xd;&#xa;    FROM&#xd;&#xa;      ETL_CONTROL_JOB &#xd;&#xa;    WHERE ID &#x3d; 11000000 -- &#x53d8;&#x91cf;&#x66ff;&#x6362;&#xd;&#xa;      AND RUN_FLAG &#x3d; 1 &#xd;&#xa;      AND JOB_TYPE IN &#x28;&#x27;DW_MYSQL&#x27;, &#x27;DW_HSQL&#x27;&#x29; --   &#x7c7b;&#x578b;&#x66ff;&#x6362;&#xd;&#xa;    &#x29; A &#xd;&#xa;    LEFT JOIN ETL_CONTROL_TASK_DW B &#xd;&#xa;      ON A.JOB_TYPE &#x3d; B.JOB_TYPE &#xd;&#xa;    LEFT JOIN ETL_CONTROL_TASK_LAST_TIME C &#xd;&#xa;      ON B.ID &#x3d; C.TASK_ID &#xd;&#xa;    LEFT JOIN ETL_CONTROL_TASK_PARENT D &#xd;&#xa;      ON B.ID &#x3d; D.TASK_ID &#xd;&#xa;    LEFT JOIN ETL_CONTROL_TASK_LAST_TIME E &#xd;&#xa;      ON D.TASK_PARENT_ID &#x3d; E.TASK_ID &#xd;&#xa;  WHERE B.RUN_FLAG &#x3d; 1 &#xd;&#xa;    AND D.IS_VALID &#x3d; 1 &#xd;&#xa;    AND &#x28;&#xd;&#xa;      &#x28;&#xd;&#xa;        UPPER&#x28;B.REFRESH_FREQ_NAME&#x29; &#x3d; UPPER&#x28;&#x27;ONCE&#x27;&#x29; &#xd;&#xa;        AND IFNULL&#x28;C.LAST_DATA_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29; &#x3d; DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#xd;&#xa;      &#x29; &#xd;&#xa;      OR &#x28;&#xd;&#xa;        UPPER&#x28;B.REFRESH_FREQ_NAME&#x29; &#x3d; UPPER&#x28;&#x27;MINUTE&#x27;&#x29; &#xd;&#xa;        AND TIMESTAMPDIFF&#x28;MINUTE,IFNULL&#x28;C.LAST_DATA_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29;,&#xd;&#xa;          SYSDATE&#x28;&#x29;&#x29; &#x3e;&#x3d; IFNULL&#x28;B.REFRESH_FREQ_NUM, 0&#x29;&#xd;&#xa;      &#x29; &#xd;&#xa;      OR &#x28;&#xd;&#xa;        UPPER&#x28;B.REFRESH_FREQ_NAME&#x29; &#x3d; UPPER&#x28;&#x27;HOURLY&#x27;&#x29; &#xd;&#xa;        AND TIMESTAMPDIFF&#x28;HOUR,IFNULL&#x28; C.LAST_DATA_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29;,&#xd;&#xa;          SYSDATE&#x28;&#x29;&#x29; &#x3e;&#x3d; IFNULL&#x28;B.REFRESH_FREQ_NUM, 0&#x29;&#xd;&#xa;      &#x29; &#xd;&#xa;      OR &#x28;&#xd;&#xa;        UPPER&#x28;B.REFRESH_FREQ_NAME&#x29; &#x3d; UPPER&#x28;&#x27;DAILY&#x27;&#x29; &#xd;&#xa;        AND IFNULL&#x28;C.LAST_DATA_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29; &#x3c; DATE&#x28;SYSDATE&#x28;&#x29;&#x29; &#xd;&#xa;        AND TIMESTAMPDIFF&#x28;MINUTE,IFNULL&#x28;CURDATE&#x28;&#x29;, DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29;,&#xd;&#xa;          SYSDATE&#x28;&#x29;&#x29; &#x3e;&#x3d; IFNULL&#x28;B.REFRESH_FREQ_NUM, 0&#x29;&#xd;&#xa;      &#x29; &#xd;&#xa;      OR &#x28;&#xd;&#xa;        UPPER&#x28;B.REFRESH_FREQ_NAME&#x29; &#x3d; UPPER&#x28;&#x27;WEEKLY&#x27;&#x29; &#xd;&#xa;        AND WEEKDAY&#x28;SYSDATE&#x28;&#x29;&#x29; &#x2b; 1 &#x3d; IFNULL&#x28;B.REFRESH_FREQ_NUM, 1&#x29; &#xd;&#xa;        AND IFNULL&#x28;C.LAST_DATA_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29; &#x3c; DATE&#x28;SYSDATE&#x28;&#x29;&#x29;&#xd;&#xa;      &#x29; &#xd;&#xa;      OR &#x28;&#xd;&#xa;        UPPER&#x28;B.REFRESH_FREQ_NAME&#x29; &#x3d; UPPER&#x28;&#x27;MONTHLY&#x27;&#x29; &#xd;&#xa;        AND DAY&#x28;SYSDATE&#x28;&#x29;&#x29; &#x3d; IFNULL&#x28;B.REFRESH_FREQ_NUM, 1&#x29; &#xd;&#xa;        AND IFNULL&#x28;C.LAST_DATA_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29; &#x3c; DATE&#x28;SYSDATE&#x28;&#x29;&#x29;&#xd;&#xa;      &#x29;&#xd;&#xa;    &#x29; &#xd;&#xa;    AND B.ID NOT IN &#xd;&#xa;    &#x28;SELECT &#xd;&#xa;      TASK_ID &#xd;&#xa;    FROM&#xd;&#xa;      ETL_CONTROL_RESOURCE_POOL P&#xd;&#xa;    WHERE TASK_STATUS IN &#x28;1, 2&#x29;&#x29; &#xd;&#xa;  GROUP BY A.ID,&#xd;&#xa;    A.JOB_NAME,&#xd;&#xa;    A.JOB_TYPE,&#xd;&#xa;    B.ID,&#xd;&#xa;    B.TASK_NAME,&#xd;&#xa;    B.TASK_ETL_TYPE,&#xd;&#xa;    DATE_ADD&#x28;IFNULL&#x28;C.LAST_DATA_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29;,INTERVAL - B.OFFSET MINUTE&#x29;,&#xd;&#xa;    IFNULL&#x28;C.LAST_DELETE_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29;,&#xd;&#xa;    IFNULL&#x28;C.LAST_MERGE_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29;,&#xd;&#xa;    IFNULL&#x28;C.LAST_DATA_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29;,&#xd;&#xa;    UPPER&#x28;B.REFRESH_FREQ_NAME&#x29;,&#xd;&#xa;    B.REFRESH_FREQ_NUM,&#xd;&#xa;    IFNULL&#x28;C.LAST_DATA_TIME,DATE&#x28;&#x27;1900-01-01&#x27;&#x29;&#x29;&#xd;&#xa;    &#x29; X&#xd;&#xa;WHERE &#x28;&#xd;&#xa;    REFRESH_FREQ_NAME IN &#x28;&#x27;DAILY&#x27;, &#x27;WEEKLY&#x27;, &#x27;MONTHLY&#x27;&#x29; &#xd;&#xa;    AND LAST_DATA_TIME_REAL &#x3c; LAST_PARENT_DATA_TIME &#xd;&#xa;    AND LAST_PARENT_DATA_TIME &#x3e;&#x3d; DATE&#x28;SYSDATE&#x28;&#x29;&#x29;&#xd;&#xa;  &#x29; &#xd;&#xa;  OR &#x28;&#xd;&#xa;    REFRESH_FREQ_NAME IN &#x28;&#x27;ONCE&#x27;, &#x27;MINUTE&#x27;, &#x27;HOURLY&#x27;&#x29; &#xd;&#xa;    AND LAST_DATA_TIME_REAL &#x3c; LAST_PARENT_DATA_TIME&#xd;&#xa;  &#x29;  &#xd;&#xa;  &#x29;AS Z&#xd;&#xa;  ORDER BY LAST_DATA_TIME_ASC ASC,TASK_PRI DESC&#xd;&#xa;  LIMIT 10 -- &#x53d8;&#x91cf;&#x66ff;&#x6362;&#xd;&#xa; &#x3b;&#xd;&#xa;&#xd;&#xa;UNLOCK TABLES&#x3b;</sql>
