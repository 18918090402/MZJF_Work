    <sql>SELECT &#xd;&#xa;    &#x24;&#x7b;T_1_0&#x7d;                AS STAT_DATE,&#xd;&#xa;    -- f.id AS order_id,&#xd;&#xa;    f.bo_id AS APPLY_NO, &#xd;&#xa;    a.ZM CREDIT_ZM,&#xd;&#xa;    CAST&#x28;td.final_score AS SIGNED&#x29; AS CREDIT_TD,&#xd;&#xa;    CASE WHEN b.z1&#x3d;&#x27;nu&#x27; THEN NULL ELSE b.z1 END MULT_INVEST_1M,&#xd;&#xa;    CASE WHEN b.z2&#x3d;&#x27;nu&#x27; THEN NULL ELSE b.z2 END MULT_INVEST_3M,&#xd;&#xa;    CASE WHEN b.z3&#x3d;&#x27;nu&#x27; THEN NULL ELSE b.z3 END MULT_INVEST_6M,&#xd;&#xa;    CASE &#xd;&#xa;    WHEN e.order_id IS NULL THEN -1&#xd;&#xa;    WHEN e.JXL&#x3e;&#x3d;0 AND LENGTH&#x28;e.JXL&#x29; &#x3d; CHAR_LENGTH&#x28;e.JXL&#x29; THEN e.JXL  &#xd;&#xa;    ELSE NULL &#xd;&#xa;    END AS CREDIT_JXL ,&#xd;&#xa;    f.credit_level CREDIT_MY,&#xd;&#xa;  &#x27;SYS&#x27;              AS DW_CREATE_BY,&#xd;&#xa;  SYSDATE&#x28;&#x29;          AS DW_CREATE_TIME,&#xd;&#xa;  &#x27;SYS&#x27;              AS DW_UPDATE_BY,&#xd;&#xa;  SYSDATE&#x28;&#x29;          AS DW_UPDATE_TIME&#xd;&#xa;    &#xd;&#xa;FROM &#xd;&#xa;    finance_order.ORD_ORDER f&#xd;&#xa;    LEFT JOIN &#xd;&#xa;    &#x28;&#xd;&#xa;        SELECT order_id,LEFT &#x28;RIGHT&#x28;DATA,5&#x29;,3&#x29; AS ZM &#xd;&#xa;        FROM finance_order.ORD_USER_HONOR&#xd;&#xa;        WHERE honor_type &#x3d;&#x27;ZHIMA&#x27; AND INSTR&#x28;UPPER&#x28;DATA&#x29;,UPPER&#x28;&#x27;zmScore&#x27;&#x29;&#x29;&#x3e;0   &#xd;&#xa;    &#x29; a &#xd;&#xa;    ON a.order_id&#x3d;f.id &#xd;&#xa;    LEFT JOIN&#xd;&#xa;    &#x28;&#xd;&#xa;        SELECT order_id,SUBSTRING_INDEX&#x28;SUBSTRING_INDEX&#x28;DATA,&#x27;finalScore&#x22;&#x3a;&#x27;,-1&#x29;,&#x27;,&#x27;,1&#x29; final_score&#xd;&#xa;        FROM finance_order.ORD_USER_HONOR&#xd;&#xa;        WHERE honor_type &#x3d;&#x27;TONGDUN&#x27; AND DATA LIKE &#x27;&#x25;finalscore&#x25;&#x27;    &#xd;&#xa;    &#x29; td &#xd;&#xa;    ON a.order_id&#x3d;td.order_id &#xd;&#xa;    LEFT JOIN&#xd;&#xa;    &#x28;&#xd;&#xa;        SELECT order_id&#xd;&#xa;		       ,TRIM&#x28;&#x27;,&#x27; FROM SUBSTRING&#x28;SUBSTRING_INDEX&#x28;DATA,&#x27;1&#x4e2a;&#x6708;&#x5185;&#x7533;&#x8bf7;&#x4eba;&#x5728;&#x591a;&#x4e2a;&#x5e73;&#x53f0;&#x7533;&#x8bf7;&#x501f;&#x6b3e;&#x27;,-1&#x29;&#xd;&#xa;                            ,LOCATE&#x28;&#x27;platform_count&#x27;,SUBSTRING_INDEX&#x28;DATA,&#x27;1&#x4e2a;&#x6708;&#x5185;&#x7533;&#x8bf7;&#x4eba;&#x5728;&#x591a;&#x4e2a;&#x5e73;&#x53f0;&#x7533;&#x8bf7;&#x501f;&#x6b3e;&#x27;,-1&#x29;&#x29;&#x2b;16,2&#x29;&#x29;z1&#xd;&#xa;			   ,TRIM&#x28;&#x27;,&#x27; FROM SUBSTRING&#x28;SUBSTRING_INDEX&#x28;DATA,&#x27;3&#x4e2a;&#x6708;&#x5185;&#x7533;&#x8bf7;&#x4eba;&#x5728;&#x591a;&#x4e2a;&#x5e73;&#x53f0;&#x7533;&#x8bf7;&#x501f;&#x6b3e;&#x27;,-1&#x29;&#xd;&#xa;                            ,LOCATE&#x28;&#x27;platform_count&#x27;,SUBSTRING_INDEX&#x28;DATA,&#x27;3&#x4e2a;&#x6708;&#x5185;&#x7533;&#x8bf7;&#x4eba;&#x5728;&#x591a;&#x4e2a;&#x5e73;&#x53f0;&#x7533;&#x8bf7;&#x501f;&#x6b3e;&#x27;,-1&#x29;&#x29;&#x2b;16,2&#x29;&#x29;Z2&#xd;&#xa;			   ,TRIM&#x28;&#x27;,&#x27; FROM SUBSTRING&#x28;SUBSTRING_INDEX&#x28;DATA,&#x27;6&#x4e2a;&#x6708;&#x5185;&#x7533;&#x8bf7;&#x4eba;&#x5728;&#x591a;&#x4e2a;&#x5e73;&#x53f0;&#x7533;&#x8bf7;&#x501f;&#x6b3e;&#x27;,-1&#x29;&#xd;&#xa;                            ,LOCATE&#x28;&#x27;platform_count&#x27;,SUBSTRING_INDEX&#x28;DATA,&#x27;6&#x4e2a;&#x6708;&#x5185;&#x7533;&#x8bf7;&#x4eba;&#x5728;&#x591a;&#x4e2a;&#x5e73;&#x53f0;&#x7533;&#x8bf7;&#x501f;&#x6b3e;&#x27;,-1&#x29;&#x29;&#x2b;16,2&#x29;&#x29;Z3&#xd;&#xa;        FROM finance_order.ORD_USER_HONOR&#xd;&#xa;        WHERE honor_type &#x3d;&#x27;TONGDUN&#x27;&#xd;&#xa;        AND &#x28;&#xd;&#xa;		      DATA LIKE &#x27;&#x25;1&#x4e2a;&#x6708;&#x5185;&#x7533;&#x8bf7;&#x4eba;&#x5728;&#x591a;&#x4e2a;&#x5e73;&#x53f0;&#x7533;&#x8bf7;&#x501f;&#x6b3e;&#x25;&#x27; &#xd;&#xa;			  OR &#xd;&#xa;			  DATA LIKE &#x27;&#x25;3&#x4e2a;&#x6708;&#x5185;&#x7533;&#x8bf7;&#x4eba;&#x5728;&#x591a;&#x4e2a;&#x5e73;&#x53f0;&#x7533;&#x8bf7;&#x501f;&#x6b3e;&#x25;&#x27; &#xd;&#xa;			  OR&#xd;&#xa;			  DATA LIKE &#x27;&#x25;6&#x4e2a;&#x6708;&#x5185;&#x7533;&#x8bf7;&#x4eba;&#x5728;&#x591a;&#x4e2a;&#x5e73;&#x53f0;&#x7533;&#x8bf7;&#x501f;&#x6b3e;&#x25;&#x27; &#xd;&#xa;			  &#x29;&#xd;&#xa;    &#x29; b &#xd;&#xa;    ON a.order_id&#x3d;b.order_id    &#xd;&#xa;    LEFT JOIN &#xd;&#xa;    &#x28;&#xd;&#xa;        SELECT order_id ,&#xd;&#xa;        MAX&#x28;CASE WHEN  &#xd;&#xa;         REPLACE&#x28;DATA,&#x27;&#x4e0e;&#x7535;&#x5546;&#x27;,&#x27;&#x27;&#x29; LIKE  &#x27;&#x25;&#x6839;&#x636e;&#x8fd0;&#x8425;&#x5546;&#x6570;&#x636e;&#x25;&#x27;&#xd;&#xa;        THEN         &#xd;&#xa;        SUBSTRING_INDEX&#x28;SUBSTRING&#x28;TRIM&#x28;&#x27;,&#x27; FROM SUBSTRING_INDEX&#x28;REPLACE&#x28;DATA,&#x27;&#x4e0e;&#x7535;&#x5546;&#x27;,&#x27;&#x27;&#x29;,&#x27;&#x6839;&#x636e;&#x8fd0;&#x8425;&#x5546;&#x6570;&#x636e;&#x27;,-1&#x29;&#x29;,19,5&#x29;,&#x27;&#x4e2a;&#x27;,1&#x29; &#xd;&#xa;        ELSE NULL END&#x29; AS JXL&#xd;&#xa;        FROM finance_order.ORD_USER_HONOR  a&#xd;&#xa;        WHERE honor_type LIKE &#x27;&#x25;JUXINLI_MOBILE&#x25;&#x27;&#xd;&#xa;        GROUP BY order_id &#xd;&#xa;    &#x29; e ON a.order_id&#x3d;e.order_id    &#xd;&#xa;&#xd;&#xa;&#x3b;</sql>
