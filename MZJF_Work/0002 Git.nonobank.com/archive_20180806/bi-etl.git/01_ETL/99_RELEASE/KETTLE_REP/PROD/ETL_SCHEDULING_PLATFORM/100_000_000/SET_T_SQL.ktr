    <sql>SELECT  CONCAT&#x28;A,A1,B,C,D&#x29;  AS T_I_SQL&#xd;&#xa;FROM &#x28;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;&#x27; SELECT    &#x27;  A,	 &#xd;&#xa;GROUP_CONCAT&#x28; &#xd;&#xa;	CASE &#xd;&#xa;	-- &#x5b57;&#x7b26;&#xd;&#xa;	WHEN INSTR&#x28;COLUMN_TYPE,&#x27;varchar&#x27;&#x29;&#x3e;0 OR INSTR&#x28;COLUMN_TYPE,&#x27;char&#x27;&#x29;&#x3e;0 OR INSTR&#x28;COLUMN_TYPE,&#x27;text&#x27;&#x29;&#x3e;0 OR INSTR&#x28;COLUMN_TYPE,&#x27;blob&#x27;&#x29;&#x3e;0&#xd;&#xa;	&#xd;&#xa;	THEN CONCAT&#x28;&#xd;&#xa;                     &#x27;REPLACE&#x28;&#x27;,&#x27;REPLACE&#x28;&#x27;,&#x27;REPLACE&#x28;&#x27;,&#x27;T.&#x27;,UPPER&#x28;CONCAT&#x28;&#x27;&#x60;&#x27;,COLUMN_NAME,&#x27;&#x60;&#x27;&#x29;&#x29;,&#x27;,CHAR&#x28;9&#x29;,&#x27;&#x27;  &#x27;&#x27;&#x29;&#x27;,&#x27;,CHAR&#x28;13&#x29;,&#x27;&#x27;  &#x27;&#x27;&#x29;&#x27;,&#x27;,CHAR&#x28;10&#x29;,&#x27;&#x27;  &#x27;&#x27;&#x29;&#x27;,&#x27; AS &#x27;,UPPER&#x28;CONCAT&#x28;&#x27;&#x60;&#x27;,COLUMN_NAME,&#x27;&#x60;&#x27;&#x29;&#x29;&#xd;&#xa;                    &#x29; &#xd;&#xa;        -- &#x65f6;&#x95f4;            &#xd;&#xa;        WHEN INSTR&#x28;COLUMN_TYPE,&#x27;datetime&#x27;&#x29;&#x3e;0 &#xd;&#xa;        THEN CONCAT&#x28;&#x27;CASE WHEN DATE_FORMAT&#x28;&#x27;,CONCAT&#x28;&#x27;&#x60;&#x27;,COLUMN_NAME,&#x27;&#x60;&#x27;&#x29;,&#x27;, &#x27;&#x27;&#x25;Y&#x27;&#x27;&#x29;&#x3d;&#x27;&#x27;0000&#x27;&#x27; THEN STR_TO_DATE&#x28;&#x27;&#x27;0001-01-01&#x27;&#x27;, &#x27;&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x27;&#x29;  ELSE &#x27;,CONCAT&#x28;&#x27;&#x60;&#x27;,COLUMN_NAME,&#x27;&#x60;&#x27;&#x29;,&#x27; END&#x27;,&#x27; AS &#x27;,UPPER&#x28;CONCAT&#x28;&#x27;&#x60;&#x27;,COLUMN_NAME,&#x27;&#x60;&#x27;&#x29;&#x29;&#x29;&#xd;&#xa;        -- int&#xd;&#xa;        WHEN INSTR&#x28;COLUMN_TYPE,&#x27;tinyint&#x27;&#x29;&#x3e;0 OR INSTR&#x28;COLUMN_TYPE,&#x27;year&#x27;&#x29;&#x3e;0 &#xd;&#xa;        THEN CONCAT&#x28;&#x27;CAST&#x28; &#x27;,CONCAT&#x28;&#x27;&#x60;&#x27;,COLUMN_NAME,&#x27;&#x60;&#x27;&#x29;,&#x27; AS SIGNED&#x29;&#x27;,&#x27; AS &#x27;,UPPER&#x28;CONCAT&#x28;&#x27;&#x60;&#x27;,COLUMN_NAME,&#x27;&#x60;&#x27;&#x29;&#x29;&#x29;  &#xd;&#xa;        ELSE CONCAT&#x28;&#x27;&#x60;&#x27;,COLUMN_NAME,&#x27;&#x60;&#x27;&#x29; END &#xd;&#xa;        ORDER BY COLUMN_ID  &#x29; A1 ,&#xd;&#xa;        &#xd;&#xa;	CONCAT&#x28;&#x27; FROM &#x27;,&#x27; &#x24;&#x7b;FROM_TABLE_SCHEMA&#x7d;&#x27;,&#x27;.&#x27;,&#x27;&#x24;&#x7b;FROM_TABLE_NAME&#x7d;&#x27;,&#x27; AS T &#x27;&#x29; B, &#xd;&#xa;	CASE WHEN &#x27;&#x24;&#x7b;TASK_ETL_TYPE&#x7d;&#x27;&#x3d;&#x27;I&#x27; THEN &#x27; WHERE &#x24;&#x7b;INCREMENT_FIELD&#x7d; &#x3e;&#x3d; &#x27;&#x27;&#x24;&#x7b;LAST_DATA_TIME&#x7d;&#x27;&#x27; &#x27; ELSE &#x27;   &#x27; END C,&#xd;&#xa;        &#x27; &#x3b; &#x27; D &#xd;&#xa;	FROM &#xd;&#xa;	ETL_CONTROL_TASK_TAB_COLUMNS &#xd;&#xa;	WHERE IS_VALID&#x3d;1 AND FLAG_FIELD IN &#x28;&#x27;new&#x27;,&#x27;changed&#x27;,&#x27;identical&#x27;&#x29;&#xd;&#xa;	AND UPPER&#x28;COLUMN_NAME&#x29; NOT IN &#x28;&#x27;DW_CREATE_BY&#x27;,&#x27;DW_CREATE_TIME&#x27;,&#x27;DW_UPDATE_BY&#x27;,&#x27;DW_UPDATE_TIME&#x27;,&#x27;IS_DELETE_FLAG&#x27;&#x29;&#xd;&#xa;    AND TASK_ID &#x3d; &#x24;&#x7b;TASK_ID&#x7d;        &#xd;&#xa;GROUP BY  TABLE_NAME &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x29; T&#xd;&#xa;&#x3b;&#xd;&#xa;</sql>
    <limit>0</limit>
