    <sql>&#x2f;&#x2a;&#x8fd8;&#x6b3e;&#x672c;&#x91d1;&#x2a;&#x2f;&#xd;&#xa;set mapreduce.job.name &#x3d; IDW_MC_METRIC_VALUE_CUR_1_03&#x3b;&#xd;&#xa;set mapreduce.job.queuename &#x3d; etl-dw&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.max.dynamic.partitions.pernode&#x3d;100000000&#x3b;&#xd;&#xa;set hive.exec.max.dynamic.partitions&#x3d;100000000&#x3b;&#xd;&#xa;set hive.exec.max.created.files&#x3d;100000000&#x3b;&#xd;&#xa;&#xd;&#xa;FROM &#xd;&#xa;&#x28;&#xd;&#xa;  select to_date&#x28;br.br_repay_time&#x29; as DATA_TIME,&#xd;&#xa;         br.biz_code_01 as BIZ,&#xd;&#xa;         br.category_name_01 AS asset_product,&#xd;&#xa;         sum&#x28;br_price_b&#x29; REPAYED_PRICE_B&#xd;&#xa;    from idw.fact_borrows_repayment br&#xd;&#xa;   where br.br_repay_time &#x3e;&#x3d; to_date&#x28;&#x27;&#x7b;LAST_DATA_TIME&#x7d;&#x27;&#x29;&#xd;&#xa;     AND br.br_repay_time &#x3c; to_date&#x28;&#x27;&#x7b;T&#x7d;&#x27;&#x29;&#xd;&#xa;     and br.br_is_repay &#x3e; 0&#xd;&#xa;   group by to_date&#x28;br.br_repay_time&#x29;, br.biz_code_01, br.category_name_01&#xd;&#xa;&#x29;OUTPUT&#xd;&#xa;INSERT OVERWRITE TABLE idw.MC_METRIC_VALUE_hist PARTITION&#x28;METRIC_ID&#x29;&#xd;&#xa;  SELECT 1 data_freq,&#xd;&#xa;         DATA_TIME,&#xd;&#xa;         &#x27;REPAYED_PRICE_B&#x27; METRIC_CODE,&#xd;&#xa;         4 dim_group_id,&#xd;&#xa;         map&#x28;&#x27;asset_product&#x27;,asset_product&#x29; METRIC_DIM,&#xd;&#xa;         REPAYED_PRICE_B METRIC_VALUE,&#xd;&#xa;         &#x27;SYS&#x27; AS DW_CREATE_BY,&#xd;&#xa;         from_unixtime&#x28;unix_timestamp&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS DW_CREATE_TIME,&#xd;&#xa;         &#x27;SYS&#x27; AS DW_UPDATE_BY,&#xd;&#xa;         from_unixtime&#x28;unix_timestamp&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS DW_UPDATE_TIME,&#xd;&#xa;		 BIZ,&#xd;&#xa;         200006 METRIC_ID&#xd;&#xa;         &#x3b;</sql>
