    <quoteString>N</quoteString>
    <sql>&#x2f;&#x2a;&#x518d;&#x501f;&#x4eba;&#x6570;&#x2a;&#x2f;&#xd;&#xa;set mapreduce.job.name &#x3d; IDW_MC_METRIC_VALUE_CUR_1_02&#x3b;&#xd;&#xa;set mapreduce.job.queuename &#x3d; etl-dw&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.max.dynamic.partitions.pernode&#x3d;100000000&#x3b;&#xd;&#xa;set hive.exec.max.dynamic.partitions&#x3d;100000000&#x3b;&#xd;&#xa;set hive.exec.max.created.files&#x3d;100000000&#x3b;&#xd;&#xa;&#xd;&#xa;ALTER TABLE idw.MC_METRIC_VALUE_CUR DROP IF EXISTS PARTITION &#x28;METRIC_ID&#x3d;200007&#x29;&#x3b;&#xd;&#xa;ALTER TABLE idw.MC_METRIC_VALUE_CUR_TMP DROP IF EXISTS PARTITION &#x28;METRIC_ID&#x3d;200007&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;FROM&#x28;&#xd;&#xa;  select t1.DATA_TIME,&#xd;&#xa;         t1.biz_code_01 as BIZ,&#xd;&#xa;         t1.user_id&#xd;&#xa;    from &#x28;select substring&#x28;max&#x28;bo_agree_time&#x29;, 1, 7&#x29; DATA_TIME,&#xd;&#xa;                 biz_code_01,&#xd;&#xa;                 user_id,&#xd;&#xa;                 max&#x28;bo_agree_time&#x29; as bo_agree_time&#xd;&#xa;            from idw.fact_borrows bo1&#xd;&#xa;           where bo_agree_time &#x3e;&#x3d; trunc&#x28;add_months&#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;,-1&#x29;,&#x27;MM&#x27;&#x29;&#xd;&#xa;             and bo_agree_time &#x3c; trunc&#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;,&#x27;MM&#x27;&#x29;&#xd;&#xa;           group by category_name_01, biz_code_01, user_id&#x29; t1&#xd;&#xa;   inner join &#x28;select substring&#x28;min&#x28;all_repayed_time&#x29;, 1, 7&#x29; DATA_TIME,&#xd;&#xa;                      biz_code_01,&#xd;&#xa;                      user_id,&#xd;&#xa;                      min&#x28;all_repayed_time&#x29; all_repayed_time&#xd;&#xa;                 from idw.fact_borrows bo2&#xd;&#xa;                where all_repayed_time &#x3e;&#x3d; trunc&#x28;add_months&#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;,-1&#x29;,&#x27;MM&#x27;&#x29;&#xd;&#xa;                  and all_repayed_time &#x3c; trunc&#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;,&#x27;MM&#x27;&#x29;&#xd;&#xa;                group by category_name_01, biz_code_01, user_id&#x29; t2&#xd;&#xa;      on t1.user_id &#x3d; t2.user_id&#xd;&#xa;     and t1.biz_code_01 &#x3d; t2.biz_code_01&#xd;&#xa;   where bo_agree_time &#x3e;&#x3d; all_repayed_time&#xd;&#xa;&#x29;OUTPUT&#xd;&#xa;INSERT OVERWRITE TABLE idw.MC_METRIC_VALUE_CUR_TMP PARTITION&#xd;&#xa;  &#x28;METRIC_ID, dim_type, BIZ&#x29;&#xd;&#xa;  SELECT 3 data_freq,&#xd;&#xa;         DATA_TIME,&#xd;&#xa;         &#x27;RE_BORROW_CNT&#x27; METRIC_CODE,&#xd;&#xa;         4 dim_group_id,&#xd;&#xa;         map&#x28;&#x29; METRIC_DIM,&#xd;&#xa;         count&#x28;distinct user_id&#x29; METRIC_VALUE,&#xd;&#xa;         200007 METRIC_ID,&#xd;&#xa;         1 as dim_type,&#xd;&#xa;         BIZ &#xd;&#xa;   group by DATA_TIME,BIZ    &#xd;&#xa;INSERT OVERWRITE TABLE idw.MC_METRIC_VALUE_CUR_TMP PARTITION&#xd;&#xa;  &#x28;METRIC_ID, dim_type, BIZ&#x29;&#xd;&#xa;  SELECT 3 data_freq,&#xd;&#xa;         DATA_TIME,&#xd;&#xa;         &#x27;RE_BORROW_CNT&#x27; METRIC_CODE,&#xd;&#xa;         4 dim_group_id,&#xd;&#xa;         map&#x28;&#x29; METRIC_DIM,&#xd;&#xa;         count&#x28;distinct user_id&#x29; METRIC_VALUE,&#xd;&#xa;         200007 METRIC_ID,&#xd;&#xa;         2 as dim_type,&#xd;&#xa;         &#x27;ALL&#x27; BIZ&#xd;&#xa;   group by DATA_TIME&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE idw.MC_METRIC_VALUE_hist PARTITION&#x28;METRIC_ID&#x29;&#xd;&#xa;  SELECT data_freq,&#xd;&#xa;         DATA_TIME,&#xd;&#xa;         METRIC_CODE,&#xd;&#xa;         4 dim_group_id,&#xd;&#xa;         METRIC_DIM,&#xd;&#xa;         METRIC_VALUE,&#xd;&#xa;         &#x27;SYS&#x27; AS DW_CREATE_BY,&#xd;&#xa;         from_unixtime&#x28;unix_timestamp&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS DW_CREATE_TIME,&#xd;&#xa;         &#x27;SYS&#x27; AS DW_UPDATE_BY,&#xd;&#xa;         from_unixtime&#x28;unix_timestamp&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS DW_UPDATE_TIME,&#xd;&#xa;         BIZ,&#xd;&#xa;		 METRIC_ID&#xd;&#xa;         &#xd;&#xa;FROM idw.MC_METRIC_VALUE_CUR_TMP WHERE METRIC_ID&#x3d;200007&#x3b;</sql>
