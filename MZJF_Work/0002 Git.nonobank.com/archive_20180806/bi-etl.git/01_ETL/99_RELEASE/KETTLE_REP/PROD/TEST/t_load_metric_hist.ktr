    <sql>&#x2f;&#x2a;&#x98ce;&#x9669;&#x7f13;&#x91ca;&#x91d1;&#x2a;&#x2f;&#xd;&#xa;set mapreduce.job.name &#x3d; IDW_MC_METRIC_VALUE_CUR_1_01&#x3b;&#xd;&#xa;set mapreduce.job.queuename &#x3d; etl-dw&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.max.dynamic.partitions.pernode&#x3d;100000000&#x3b;&#xd;&#xa;set hive.exec.max.dynamic.partitions&#x3d;100000000&#x3b;&#xd;&#xa;set hive.exec.max.created.files&#x3d;100000000&#x3b;&#xd;&#xa;&#xd;&#xa;FROM &#xd;&#xa;&#x28;&#xd;&#xa;  select stat_date DATA_TIME,&#xd;&#xa;         &#x27;ALL&#x27; BIZ,&#xd;&#xa;          SUM&#x28;balance&#x29; RISK_RELEASE_AMT -- &#x98ce;&#x9669;&#x7f13;&#x91ca;&#x91d1;&#xd;&#xa;FROM dmt.fact_finance_account_snapshot&#xd;&#xa;WHERE role_id IN&#x28;6,24&#x29;&#xd;&#xa;AND id IN&#x28;5,27&#x29;&#xd;&#xa;AND stat_date&#x3e;&#x3d;to_date&#x28;&#x27;&#x7b;LAST_DATA_TIME&#x7d;&#x27;&#x29;&#xd;&#xa;AND stat_date&#x3c;to_date&#x28;&#x27;&#x7b;T&#x7d;&#x27;&#x29;&#xd;&#xa;GROUP BY stat_date&#xd;&#xa;&#x29; OUTPUT&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE idw.MC_METRIC_VALUE_hist PARTITION&#x28;METRIC_ID&#x29;&#xd;&#xa;  SELECT 1 data_freq,&#xd;&#xa;         DATA_TIME,&#xd;&#xa;         &#x27;RISK_RELEASE_AMT&#x27; METRIC_CODE,&#xd;&#xa;         null dim_group_id,&#xd;&#xa;         map&#x28;&#x29; METRIC_DIM,&#xd;&#xa;         RISK_RELEASE_AMT METRIC_VALUE,&#xd;&#xa;         &#x27;SYS&#x27; AS DW_CREATE_BY,&#xd;&#xa;         from_unixtime&#x28;unix_timestamp&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS DW_CREATE_TIME,&#xd;&#xa;         &#x27;SYS&#x27; AS DW_UPDATE_BY,&#xd;&#xa;         from_unixtime&#x28;unix_timestamp&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS DW_UPDATE_TIME,&#xd;&#xa;		 BIZ,&#xd;&#xa;         900000 METRIC_ID</sql>
