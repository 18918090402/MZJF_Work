    <sql>SELECT&#xd;&#xa;  str_to_date&#x28;&#x27;&#x24;&#x7b;V_T&#x7d;&#x27;,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29;  as DATA_TAG_DATE,&#xd;&#xa;  count&#x28;distinct user_id&#x29;           as DATA_TAG_VALUE,&#xd;&#xa;  25                           AS DATA_TAG_CODE_ID,&#xd;&#xa;  &#x27;liangzai&#x27;                    AS  DW_CREATE_BY ,&#xd;&#xa;  SYSDATE&#x28;&#x29;                     AS  DW_CREATE_TIME,&#xd;&#xa;  &#x27;liangzai&#x27;                    AS  DW_UPDATE_BY,&#xd;&#xa;  SYSDATE&#x28;&#x29;                     AS  DW_UPDATE_TIME&#xd;&#xa;FROM &#x28;&#xd;&#xa; -- &#x4f18;&#x9009;&#xd;&#xa; SELECT&#xd;&#xa;    DATE&#x28;t2.create_time&#x29; AS dt,&#xd;&#xa;    t1.amount AS val,&#xd;&#xa;    t1.user_id&#xd;&#xa; FROM db_nono.vip_account t1&#xd;&#xa; JOIN &#x28;&#xd;&#xa;      SELECT&#xd;&#xa;        create_time,&#xd;&#xa;        key_value&#xd;&#xa;       FROM db_nono.finance_log&#xd;&#xa;        WHERE OPERATION_RELATION &#x3d; 598&#xd;&#xa;        AND table_name &#x3d; &#x27;vip_account&#x27;&#xd;&#xa;        AND CREATE_TIME &#x3c; &#x27;&#x24;&#x7b;V_T&#x2b;1&#x7d;&#x27;&#xd;&#xa; &#x29; t2&#xd;&#xa; ON t1.id &#x3d; t2.key_value&#xd;&#xa; AND t1.is_cash &#x3d; 1&#xd;&#xa;   UNION ALL&#xd;&#xa; -- &#x503a;&#x8f6c;&#xd;&#xa; SELECT DATE&#x28;SUCCESS_TIME&#x29; AS dt,&#xd;&#xa;        PRICE_PRINCIPAL AS val,&#xd;&#xa;        user_id&#xd;&#xa; FROM db_nono.borrows_accept&#xd;&#xa; WHERE IS_PAY&#x3d;1&#xd;&#xa; AND IS_VIP&#x3d;0&#xd;&#xa; AND SUCCESS_TIME &#x3c; &#x27;&#x24;&#x7b;V_T&#x2b;1&#x7d;&#x27;&#xd;&#xa;&#x29; t3&#xd;&#xa;join db_nono.user_info t4&#xd;&#xa;ON t3.USER_ID &#x3d; t4.ID&#xd;&#xa;WHERE t4.ID NOT IN&#x28;920272,1598,1600,1176674&#x29;&#xd;&#xa;AND t4.IS_SPECIAL&#x3c;&#x3e;1&#xd;&#xa;group by 1&#x3b;</sql>
