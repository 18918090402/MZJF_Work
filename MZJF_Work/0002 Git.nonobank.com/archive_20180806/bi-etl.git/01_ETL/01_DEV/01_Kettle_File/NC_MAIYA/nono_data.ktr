    <sql>SELECT &#xd;&#xa;bo.id AS &#x27;&#x6807;ID&#x27;, &#xd;&#xa;p.&#x60;p_name&#x60; AS &#x27;&#x4ea7;&#x54c1;&#x27;,&#xd;&#xa;ui.&#x60;real_name&#x60; AS &#x27;&#x501f;&#x6b3e;&#x59d3;&#x540d;&#x27;,&#xd;&#xa;ui.&#x60;user_name&#x60; AS &#x27;&#x7528;&#x6237;&#x540d;&#x27;,&#xd;&#xa;      ui.id_num       AS &#x22;&#x8eab;&#x4efd;&#x8bc1;&#x53f7;&#x22;&#xd;&#xa;      ,ui.mobile_num    AS &#x22;&#x624b;&#x673a;&#x53f7;&#x22;&#xd;&#xa;      ,mfd2.current_address  AS &#x22;&#x5c45;&#x4f4f;&#x5730;&#x5740;&#x22;&#xd;&#xa;      ,mfd2.register_address     AS &#x22;&#x6237;&#x7c4d;&#x5730;&#x5740;&#x22;&#xd;&#xa;      ,mfd2.current_address     AS &#x22;&#x5bb6;&#x5ead;&#x5730;&#x5740;&#x22;,&#xd;&#xa;	  bo.bo_finish_price AS &#x27;&#x501f;&#x6b3e;&#x91d1;&#x989d;&#x27;,&#xd;&#xa;br1.&#x6700;&#x521d;&#x8fd8;&#x6b3e;&#x65e5;  AS &#x27;&#x6700;&#x521d;&#x8fd8;&#x6b3e;&#x65e5;&#x27;,&#xd;&#xa;	  br1.&#x8d77;&#x59cb;&#x8fd8;&#x6b3e;&#x65e5; AS &#x27;&#x8d77;&#x59cb;&#x8fd8;&#x6b3e;&#x65e5;&#x27;,&#xd;&#xa;&#xd;&#xa;	  bo.&#x60;bo_expect&#x60; AS &#x27;&#x501f;&#x6b3e;&#x671f;&#x9650;&#x27;, &#xd;&#xa;	  bo.&#x60;bo_audit_time&#x60; AS &#x27;&#x653e;&#x6b3e;&#x65f6;&#x95f4;&#x27;,&#xd;&#xa;SUM&#x28;CASE WHEN DATE&#x28;br.br_time&#x29;&#x3c;&#x3d; CURDATE&#x28;&#x29; THEN &#x27;1&#x27; ELSE &#x27;0 &#x27; END &#x29;AS &#x27;&#x5e94;&#x8fd8;&#x671f;&#x6570;&#x27;,&#xd;&#xa;br1.&#x5b9e;&#x9645;&#x5df2;&#x8fd8;&#x671f;&#x6570; AS &#x27;&#x5b9e;&#x9645;&#x5df2;&#x8fd8;&#x671f;&#x6570;&#x27;,&#xd;&#xa;br1.&#x903e;&#x671f;&#x672a;&#x8fd8;&#x671f;&#x6570;  AS &#x27;&#x903e;&#x671f;&#x672a;&#x8fd8;&#x671f;&#x6570;&#x27;,&#xd;&#xa;MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29; AS &#x22;&#x622a;&#x6b62;&#x76ee;&#x524d;&#x4e3a;&#x6b62;&#x7684;&#x903e;&#x671f;&#x5929;&#x6570;&#x22;,    &#x2f;&#x2a; &#x622a;&#x6b62;&#x76ee;&#x524d;&#x4e3a;&#x6b62;&#x7684;&#x903e;&#x671f;&#x5929;&#x6570; &#x2a;&#x2f;&#xd;&#xa;CASE WHEN MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3e;&#x3d;&#x27;1&#x27;&#xd;&#xa;AND MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3c;&#x3d;&#x27;30&#x27; THEN &#x27;M1&#x27;&#xd;&#xa;WHEN MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3e;&#x3d;&#x27;31&#x27;&#xd;&#xa;AND MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3c;&#x3d;&#x27;60&#x27; THEN &#x27;M2&#x27;&#xd;&#xa;WHEN MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3e;&#x3d;&#x27;61&#x27;&#xd;&#xa;AND MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3c;&#x3d;&#x27;90&#x27; THEN &#x27;M3&#x27;&#xd;&#xa;WHEN MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3e;&#x3d;&#x27;91&#x27;&#xd;&#xa;AND MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3c;&#x3d;&#x27;120&#x27; THEN &#x27;M4&#x27;&#xd;&#xa;WHEN MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3e;&#x3d;&#x27;121&#x27;&#xd;&#xa;AND MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3c;&#x3d;&#x27;150&#x27; THEN &#x27;M5&#x27;&#xd;&#xa;WHEN MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3e;&#x3d;&#x27;151&#x27;&#xd;&#xa;AND MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3c;&#x3d;&#x27;180&#x27; THEN &#x27;M6&#x27;&#xd;&#xa;WHEN MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3e;&#x3d;&#x27;181&#x27;&#xd;&#xa;AND MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3c;&#x3d;&#x27;210&#x27; THEN &#x27;M7&#x27;&#xd;&#xa;WHEN MAX&#x28;CASE WHEN br.br_overdue_terms&#x3e;&#x22;0&#x22;  AND br_is_repay&#x3d;&#x22;0&#x22; THEN DATEDIFF&#x28;CURDATE&#x28;&#x29;,br.br_time&#x29; END&#x29;&#x3e;&#x3d;&#x27;211&#x27; THEN &#x27;M7&#x2b;&#x27;&#xd;&#xa;END  AS &#x27;&#x8d26;&#x9f84;&#x27;,&#xd;&#xa; br1.&#x903e;&#x671f;&#x603b;&#x989d; AS &#x27;&#x903e;&#x671f;&#x603b;&#x989d;&#x27; &#xd;&#xa; ,br1.&#x672a;&#x8fd8;&#x603b;&#x989d; AS &#x27;&#x672a;&#x8fd8;&#x603b;&#x989d;&#x27;&#xd;&#xa;  ,br1.&#x5269;&#x4f59;&#x672c;&#x91d1; AS &#x27;&#x5269;&#x4f59;&#x672c;&#x91d1;&#x27;&#xd;&#xa;  ,br1.&#x5269;&#x4f59;&#x7f5a;&#x606f;&#x548c;&#x8fdd;&#x7ea6;&#x91d1; AS &#x27;&#x5269;&#x4f59;&#x7f5a;&#x606f;&#x548c;&#x8fdd;&#x7ea6;&#x91d1;&#x27;  &#xd;&#xa;, br1.&#x6708;&#x8fd8;&#x6b3e;&#x91d1;&#x989d; AS &#x27;&#x6708;&#x8fd8;&#x6b3e;&#x91d1;&#x989d;&#x27;&#xd;&#xa; ,fa.balance AS &#x22;&#x8d26;&#x6237;&#x4f59;&#x989d;&#x22;&#xd;&#xa;      ,bo.bo_rate AS &#x22;&#x501f;&#x6b3e;&#x5e74;&#x5229;&#x7387;&#x22;&#xd;&#xa; ,&#x27;&#x27; AS &#x22;&#x516c;&#x53f8;&#x540d;&#x79f0;&#x22;&#xd;&#xa;    ,&#x27;&#x27; AS &#x22;&#x804c;&#x4f4d;&#x22;&#xd;&#xa;    ,&#x27;0&#x27; AS &#x22;&#x85aa;&#x6c34;&#x22;&#xd;&#xa;    ,&#x27;&#x27; AS &#x22;&#x76f4;&#x7cfb;&#x4eb2;&#x5c5e;&#x59d3;&#x540d;&#x22;&#xd;&#xa;   ,&#x27;&#x27; AS &#x22;&#x76f4;&#x7cfb;&#x4eb2;&#x5c5e;&#x7535;&#x8bdd;&#x22;&#xd;&#xa;   ,r2.&#x60;name&#x60; AS &#x27;&#x6ce8;&#x518c;&#x6240;&#x5728;&#x7701;&#x4efd;&#x27;,&#xd;&#xa;r1.&#x60;name&#x60; AS &#x27;&#x6ce8;&#x518c;&#x6240;&#x5728;&#x57ce;&#x5e02;&#x27;&#xd;&#xa;,NOW&#x28;&#x29; AS &#x27;&#x53d6;&#x6570;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;FROM db_nono.borrows  bo&#xd;&#xa;JOIN db_nono.user_info ui ON bo.user_id &#x3d; ui.id &#xd;&#xa;LEFT JOIN  db_nono.user_student_info mfd1 ON mfd1.user_id &#x3d; ui.id&#xd;&#xa;  LEFT JOIN db_nono.user_info_detail mfd2 ON mfd2.user_id &#x3d; ui.id&#xd;&#xa; LEFT JOIN db_nono.region r1 ON mfd2.current_city&#x3d;r1.code &#xd;&#xa; LEFT JOIN db_nono.region r2 ON mfd2.current_province&#x3d;r2.code &#xd;&#xa;LEFT JOIN db_nono.borrows_repayment br ON bo.&#x60;id&#x60;&#x3d;br.&#x60;bo_id&#x60;&#xd;&#xa;LEFT JOIN db_nono.products p ON bo.&#x60;p_id&#x60;&#x3d;p.&#x60;id&#x60;&#xd;&#xa;LEFT JOIN db_nono.borrows_consulted_fee bcf ON bo.id&#x3d;bcf.bo_id&#xd;&#xa;LEFT JOIN &#x28;&#xd;&#xa;SELECT br.bo_id,&#xd;&#xa;MAX&#x28;br.br_time&#x29; AS &#x27;&#x5230;&#x671f;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,MIN&#x28;br.br_time&#x29; AS &#x27;&#x6700;&#x521d;&#x8fd8;&#x6b3e;&#x65e5;&#x27;&#xd;&#xa;, MAX&#x28;br.br_repay_time&#x29; AS &#x22;&#x6700;&#x8fd1;&#x8fd8;&#x6b3e;&#x65e5;&#x671f;&#x22;,&#xd;&#xa; br2.br_price  AS &#x27;&#x6708;&#x8fd8;&#x6b3e;&#x91d1;&#x989d;&#x27;,&#xd;&#xa;MIN&#x28;br.br_repay_time&#x29; AS &#x27;&#x8d77;&#x59cb;&#x8fd8;&#x6b3e;&#x65e5;&#x27;,&#xd;&#xa;SUM&#x28;CASE WHEN br.br_is_repay&#x3d;&#x27;1&#x27; AND br.br_time &#x3c;NOW&#x28;&#x29; THEN &#x27;1&#x27; ELSE &#x27;0&#x27; END &#x29; AS &#x27;&#x5b9e;&#x9645;&#x5df2;&#x8fd8;&#x671f;&#x6570;&#x27;,&#xd;&#xa;SUM&#x28;CASE WHEN br.br_is_repay&#x3d;&#x27;0&#x27; AND br.br_overdue_terms&#x3e;0 THEN &#x27;1&#x27; ELSE &#x27;0&#x27; END &#x29; AS &#x27;&#x903e;&#x671f;&#x672a;&#x8fd8;&#x671f;&#x6570;&#x27;,&#xd;&#xa;SUM&#x28;CASE WHEN br.br_is_repay&#x3d;&#x27;0&#x27; THEN br.br_price_b END&#x29; AS &#x27;&#x5269;&#x4f59;&#x672c;&#x91d1;&#x27;,&#xd;&#xa;SUM&#x28;CASE WHEN br.br_is_repay&#x3d;&#x27;0&#x27; THEN br.br_price_punish END&#x29; AS &#x22;&#x5269;&#x4f59;&#x7f5a;&#x606f;&#x548c;&#x8fdd;&#x7ea6;&#x91d1;&#x22;,&#xd;&#xa;SUM&#x28;CASE WHEN br.br_is_repay&#x3d;&#x27;0&#x27; AND  br.br_overdue_terms&#x3e;0 THEN br.br_price&#x2b;br.br_price_punish END&#x29; AS &#x27;&#x903e;&#x671f;&#x603b;&#x989d;&#x27;,&#xd;&#xa;SUM&#x28;CASE WHEN br.br_is_repay&#x3d;0 THEN br.br_price&#x2b;br.br_price_punish END &#x29; AS &#x27;&#x672a;&#x8fd8;&#x603b;&#x989d;&#x27;&#xd;&#xa;FROM db_nono.borrows_repayment br&#xd;&#xa;LEFT JOIN &#x28;SELECT bo_id,br_price FROM db_nono.borrows_repayment br2 WHERE br_expect_num&#x3d;1&#x29; br2 ON br2.bo_id&#x3d;br.bo_id &#xd;&#xa;GROUP BY br.bo_id   &#xd;&#xa;&#x29; br1 ON bo.id&#x3d;br1.bo_id &#xd;&#xa;LEFT JOIN  db_nono.finance_account fa  ON fa.user_id&#x3d;ui.id AND fa.role_id &#x3d;7 &#xd;&#xa;WHERE &#xd;&#xa;bo.bo_is_success&#x3d;&#x27;1&#x27; AND bo.p_id IN &#x28;&#x27;96&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;GROUP BY 1</sql>
