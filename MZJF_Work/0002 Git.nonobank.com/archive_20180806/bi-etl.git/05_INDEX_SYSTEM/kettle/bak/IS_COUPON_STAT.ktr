    <connection>nono</connection>
    <sql>SELECT&#xd;&#xa;  tt.coupon_date                 AS dt,&#xd;&#xa;  tt.coupon_name                 AS coupon_name,&#xd;&#xa;  cash_value&#x2b;rate_value          AS cost_amt,&#xd;&#xa;  tt.inv_amt                     AS inv_amt,&#xd;&#xa;  tt.usr_cnt,&#xd;&#xa;  tt.coupon_cnt&#xd;&#xa;FROM &#x28;&#xd;&#xa; SELECT&#xd;&#xa;  DATE&#x28;vf.service_time&#x29;                           AS coupon_date,&#xd;&#xa;  TRIM&#x28;IF&#x28;uc.approach_desc IS NULL OR uc.approach_desc &#x3d; &#x27;&#x27;,uc.description,uc.approach_desc&#x29;&#x29;   AS coupon_name,&#xd;&#xa;  SUM&#x28;IF&#x28;uc.type&#x3d;0,uc.value,0&#x29;&#x29;                   AS cash_value,&#xd;&#xa;  ROUND&#x28;SUM&#x28;IF&#x28;uc.type&#x3d;1,IF&#x28;fp.expect_unit&#x3d;0,CASE WHEN vf.MODE IN&#x28;8,9,10,11&#x29; THEN amount_payed&#x2a; &#x28;CASE WHEN vf.MODE&#x3d;8 THEN 3 WHEN vf.MODE&#x3d;9 THEN 6 WHEN vf.MODE&#x3d;10 THEN 12 WHEN vf.MODE&#x3d;11 THEN 24 ELSE NULL END&#x29;&#x2f;12 ELSE amount_payed&#x2a;IFNULL&#x28;fp.expect,12&#x29;&#x2f;12 END,amount_payed&#x2a;fp.expect&#x2f;360&#x29;&#x2a;uc.value&#x2f;100,0&#x29;&#x29;,2&#x29; AS rate_value,&#xd;&#xa;  SUM&#x28;vf.amount_payed&#x29; AS inv_amt,&#xd;&#xa;  COUNT&#x28;DISTINCT uc.user_id&#x29; AS usr_cnt,&#xd;&#xa;  COUNT&#x28;uc.id&#x29; AS coupon_cnt&#xd;&#xa;FROM db_nono.coupon_use cu&#xd;&#xa;  JOIN db_nono.user_coupon uc&#xd;&#xa;    ON uc.id &#x3d; cu.uv_id&#xd;&#xa;      AND cu.ir_product_type &#x3d; 1&#xd;&#xa;  JOIN db_nono.vip_form vf&#xd;&#xa;    ON vf.id &#x3d; cu.bb_id&#xd;&#xa;    AND vf.service_status &#x3d; 1&#xd;&#xa;  JOIN db_nono.user_info ui&#xd;&#xa;    ON ui.id &#x3d; vf.user_id&#xd;&#xa;  LEFT JOIN db_nono.finance_plan fp&#xd;&#xa;    ON vf.fp_id &#x3d; fp.id&#xd;&#xa; -- WHERE &#xd;&#xa; --   vf.service_time &#x3e;&#x3d; &#x27;&#x24;&#x7b;V_T&#x7d;&#x27;&#xd;&#xa; --   AND vf.service_time &#x3c; &#x27;&#x24;&#x7b;V_T&#x2b;1&#x7d;&#x27;&#xd;&#xa;GROUP BY 1,2&#xd;&#xa;&#x29; tt</sql>
    <limit>0</limit>
