    <sql>select&#xd;&#xa; ttt.INDEX_ID,&#xd;&#xa; ttt.INDEX_GROUP,&#xd;&#xa; ttt.INDEX_NAME,&#xd;&#xa; ttt.INDEX_REPORT,&#xd;&#xa; ttt.SQL_DB,&#xd;&#xa;&#xd;&#xa; case&#xd;&#xa;  when ttt.DIM_CNT is null or ttt.DIM_CNT &#x3d; 0&#xd;&#xa;  then replace&#x28;ttt.SQL_TEXT,&#x27;&#x23;&#x23;dim_list&#x23;&#x23;&#x27;,&#x27;&#x27;&#x27;&#x27;&#x27; AS dim_1_nm,&#x27;&#x27;&#x27;&#x27; AS dim_2_nm,&#x27;&#x27;&#x27;&#x27; AS dim_3_nm,&#x27;&#x27;&#x27;&#x27; AS dim_4_nm,&#x27;&#x27;&#x27;&#x27; AS dim_5_nm&#x27;&#x29;&#xd;&#xa;  when ttt.DIM_CNT &#x3d; 1&#xd;&#xa;  then concat&#x28;replace&#x28;ttt.SQL_TEXT,&#x27;&#x23;&#x23;dim_list&#x23;&#x23;&#x27;,concat&#x28;ttt.DIM_1_NM,&#x27; as dim_1_nm,&#x27;&#x27;&#x27;&#x27; AS dim_2_nm,&#x27;&#x27;&#x27;&#x27; AS dim_3_nm,&#x27;&#x27;&#x27;&#x27; AS dim_4_nm,&#x27;&#x27;&#x27;&#x27; AS dim_5_nm&#x27;&#x29;&#x29;,&#x27; group by &#x27;,ttt.DIM_1_NM&#x29;&#xd;&#xa;  when ttt.DIM_CNT &#x3d; 2&#xd;&#xa;  then concat&#x28;replace&#x28;ttt.SQL_TEXT,&#x27;&#x23;&#x23;dim_list&#x23;&#x23;&#x27;,concat&#x28;ttt.DIM_1_NM,&#x27; as dim_1_nm,&#x27;,ttt.DIM_2_NM,&#x27; as dim_2_nm,&#x27;&#x27;&#x27;&#x27; AS dim_3_nm,&#x27;&#x27;&#x27;&#x27; AS dim_4_nm,&#x27;&#x27;&#x27;&#x27; AS dim_5_nm&#x27;&#x29;&#x29;,&#x27; group by &#x27;,ttt.DIM_1_NM,&#x27;,&#x27;,ttt.DIM_2_NM&#x29;&#xd;&#xa;  when ttt.DIM_CNT &#x3d; 3&#xd;&#xa;  then concat&#x28;replace&#x28;ttt.SQL_TEXT,&#x27;&#x23;&#x23;dim_list&#x23;&#x23;&#x27;,concat&#x28;ttt.DIM_1_NM,&#x27; as dim_1_nm,&#x27;,ttt.DIM_2_NM,&#x27; as dim_2_nm,&#x27;,ttt.DIM_3_NM,&#x27; as dim_3_nm,&#x27;&#x27;&#x27;&#x27; AS dim_4_nm,&#x27;&#x27;&#x27;&#x27; AS dim_5_nm&#x27;&#x29;&#x29;,&#x27; group by &#x27;,ttt.DIM_1_NM,&#x27;,&#x27;,ttt.DIM_2_NM,&#x27;,&#x27;,ttt.DIM_3_NM&#x29;&#xd;&#xa;  when ttt.DIM_CNT &#x3d; 4&#xd;&#xa;  then concat&#x28;replace&#x28;ttt.SQL_TEXT,&#x27;&#x23;&#x23;dim_list&#x23;&#x23;&#x27;,concat&#x28;ttt.DIM_1_NM,&#x27; as dim_1_nm,&#x27;,ttt.DIM_2_NM,&#x27; as dim_2_nm,&#x27;,ttt.DIM_3_NM,&#x27; as dim_3_nm,&#x27;,ttt.DIM_4_NM,&#x27; as dim_4_nm,&#x27;&#x27;&#x27;&#x27; AS dim_5_nm&#x27;&#x29;&#x29;,&#x27; group by &#x27;,ttt.DIM_1_NM,&#x27;,&#x27;,ttt.DIM_2_NM,&#x27;,&#x27;,ttt.DIM_3_NM,&#x27;,&#x27;,ttt.DIM_4_NM&#x29;&#xd;&#xa;  when ttt.DIM_CNT &#x3d; 5&#xd;&#xa;  then concat&#x28;replace&#x28;ttt.SQL_TEXT,&#x27;&#x23;&#x23;dim_list&#x23;&#x23;&#x27;,concat&#x28;ttt.DIM_1_NM,&#x27; as dim_1_nm,&#x27;,ttt.DIM_2_NM,&#x27; as dim_2_nm,&#x27;,ttt.DIM_3_NM,&#x27; as dim_3_nm,&#x27;,ttt.DIM_4_NM,&#x27; as dim_4_nm,&#x27;,ttt.DIM_5_NM,&#x27; as dim_5_nm&#x27;&#x29;&#x29;,&#x27; group by &#x27;,ttt.DIM_1_NM,&#x27;,&#x27;,ttt.DIM_2_NM,&#x27;,&#x27;,ttt.DIM_3_NM,&#x27;,&#x27;,ttt.DIM_4_NM,&#x27;,&#x27;,ttt.DIM_5_NM&#x29;&#xd;&#xa; end SQL_TEXT,&#xd;&#xa;&#xd;&#xa; ttt.MEASURE_TYPE,&#xd;&#xa; ttt.DIM_CNT,&#xd;&#xa; ttt.MEASURE_TYPE_DESC,&#xd;&#xa;&#xd;&#xa; case&#xd;&#xa;    when ttt.DIM_CNT is null or ttt.DIM_CNT &#x3d; 0&#xd;&#xa;    THEN ttt.MEASURE_TYPE_DESC&#xd;&#xa;    when ttt.DIM_CNT &#x3d; 1&#xd;&#xa;    then concat_ws&#x28;&#x27;,&#x27;,ttt.DIM_1_NM,ttt.MEASURE_TYPE_DESC&#x29;&#xd;&#xa;    when ttt.DIM_CNT &#x3d; 2&#xd;&#xa;    then concat_ws&#x28;&#x27;,&#x27;,ttt.DIM_1_NM,ttt.DIM_2_NM,ttt.MEASURE_TYPE_DESC&#x29;&#xd;&#xa;    when ttt.DIM_CNT &#x3d; 3&#xd;&#xa;    then concat_ws&#x28;&#x27;,&#x27;,ttt.DIM_1_NM,ttt.DIM_2_NM,ttt.DIM_3_NM,ttt.MEASURE_TYPE_DESC&#x29;&#xd;&#xa;    when ttt.DIM_CNT &#x3d; 4&#xd;&#xa;    then concat_ws&#x28;&#x27;,&#x27;,ttt.DIM_1_NM,ttt.DIM_2_NM,ttt.DIM_3_NM,ttt.DIM_4_NM,ttt.MEASURE_TYPE_DESC&#x29;&#xd;&#xa;    when ttt.DIM_CNT &#x3d; 5&#xd;&#xa;    then concat_ws&#x28;&#x27;,&#x27;,ttt.DIM_1_NM,ttt.DIM_2_NM,ttt.DIM_3_NM,ttt.DIM_4_NM,ttt.DIM_5_NM,ttt.MEASURE_TYPE_DESC&#x29;&#xd;&#xa;  end INDEX_KEY&#xd;&#xa;&#xd;&#xa;from &#x28;&#xd;&#xa;select&#xd;&#xa; tt.INDEX_ID,&#xd;&#xa; tt.INDEX_GROUP,&#xd;&#xa; tt.INDEX_NAME,&#xd;&#xa; tt.INDEX_REPORT,&#xd;&#xa; tt.SQL_DB,&#xd;&#xa;&#xd;&#xa; replace&#x28;replace&#x28;replace&#x28;replace&#x28;replace&#x28;replace&#x28;&#xd;&#xa; case&#xd;&#xa;   when tt.SQL_DB in &#x28;&#x27;hive&#x27;&#x29;&#xd;&#xa;   then &#x27;&#x24;&#x7b;V_HIVE_TEMPLATE&#x7d;&#x27;&#xd;&#xa;   when tt.SQL_DB in &#x28;&#x27;impala&#x27;&#x29;&#xd;&#xa;   then &#x27;&#x24;&#x7b;V_IMPALA_TEMPLATE&#x7d;&#x27;&#xd;&#xa;   else &#x27;&#x24;&#x7b;V_MYSQL_TEMPLATE&#x7d;&#x27;&#xd;&#xa; end,&#xd;&#xa; &#x27;&#x23;&#x23;stat_date&#x23;&#x23;&#x27;,tt.stat_date&#x29;,&#x27;&#x23;&#x23;begin_date&#x23;&#x23;&#x27;,tt.begin_date&#x29;,&#x27;&#x23;&#x23;end_date&#x23;&#x23;&#x27;,tt.end_date&#x29;,&#x27;&#x23;&#x23;measure_type&#x23;&#x23;&#x27;,tt.MEASURE_TYPE&#x29;,&#x27;&#x23;&#x23;sql_text&#x23;&#x23;&#x27;,&#xd;&#xa; replace&#x28;replace&#x28;replace&#x28;tt.sql_text,&#x27;&#x24;&#x7b;V_STAT_DATE&#x7d;&#x27;,tt.stat_date&#x29;,&#x27;&#x24;&#x7b;V_BEGIN_DATE&#x7d;&#x27;,tt.begin_date&#x29;,&#x27;&#x24;&#x7b;V_END_DATE&#x7d;&#x27;,tt.end_date&#x29;&#xd;&#xa; &#x29;,&#x27;&#x23;&#x23;date_column&#x23;&#x23;&#x27;,tt.date_column&#x29; as SQL_TEXT,&#xd;&#xa;&#xd;&#xa; tt.DIM_1_NM,&#xd;&#xa; tt.DIM_2_NM,&#xd;&#xa; tt.DIM_3_NM,&#xd;&#xa; tt.DIM_4_NM,&#xd;&#xa; tt.DIM_5_NM,&#xd;&#xa; tt.DIM_CNT,&#xd;&#xa; tt.MEASURE_TYPE,&#xd;&#xa; tt.MEASURE_TYPE_DESC&#xd;&#xa;from &#x28;&#xd;&#xa;select&#xd;&#xa; t1.INDEX_ID,&#xd;&#xa; t1.INDEX_GROUP,&#xd;&#xa; t1.INDEX_NAME,&#xd;&#xa; t1.INDEX_REPORT,&#xd;&#xa; t1.SQL_DB,&#xd;&#xa;&#xd;&#xa; case&#xd;&#xa;  when t2.PATTERN_ID is not null&#xd;&#xa;  then replace&#x28;t1.SQL_TEXT,&#x27;&#x24;&#x7b;V_SQL_PATTERN&#x7d;&#x27;,t2.PATTERN_TEXT&#x29;&#xd;&#xa;  else t1.SQL_TEXT&#xd;&#xa; end SQL_TEXT,&#xd;&#xa;&#xd;&#xa; ifnull&#x28;SUBSTRING_INDEX&#x28;t4.DIM_COL_LIST,&#x27;,&#x27;,1&#x29;,t1.DIM_1_NM&#x29;                                                                           AS  DIM_1_NM,&#xd;&#xa; ifnull&#x28;TRIM&#x28;LEADING &#x27;,&#x27; FROM REPLACE&#x28;SUBSTRING_INDEX&#x28;t4.DIM_COL_LIST,&#x27;,&#x27;,2&#x29;,SUBSTRING_INDEX&#x28;t4.DIM_COL_LIST,&#x27;,&#x27;,1&#x29;,&#x27;&#x27;&#x29;&#x29;,t1.DIM_2_NM&#x29; AS  DIM_2_NM,&#xd;&#xa; ifnull&#x28;TRIM&#x28;LEADING &#x27;,&#x27; FROM REPLACE&#x28;SUBSTRING_INDEX&#x28;t4.DIM_COL_LIST,&#x27;,&#x27;,3&#x29;,SUBSTRING_INDEX&#x28;t4.DIM_COL_LIST,&#x27;,&#x27;,2&#x29;,&#x27;&#x27;&#x29;&#x29;,t1.DIM_3_NM&#x29; AS  DIM_3_NM,&#xd;&#xa; TRIM&#x28;LEADING &#x27;,&#x27; FROM REPLACE&#x28;SUBSTRING_INDEX&#x28;t4.DIM_COL_LIST,&#x27;,&#x27;,4&#x29;,SUBSTRING_INDEX&#x28;t4.DIM_COL_LIST,&#x27;,&#x27;,3&#x29;,&#x27;&#x27;&#x29;&#x29;                     AS  DIM_4_NM,&#xd;&#xa; TRIM&#x28;LEADING &#x27;,&#x27; FROM REPLACE&#x28;SUBSTRING_INDEX&#x28;t4.DIM_COL_LIST,&#x27;,&#x27;,5&#x29;,SUBSTRING_INDEX&#x28;t4.DIM_COL_LIST,&#x27;,&#x27;,4&#x29;,&#x27;&#x27;&#x29;&#x29;                     AS  DIM_5_NM,&#xd;&#xa;&#xd;&#xa; ifnull&#x28;t4.DIM_CNT,t1.DIM_CNT&#x29; AS DIM_CNT,&#xd;&#xa;&#xd;&#xa; t3.MEA_EXPR    AS MEASURE_TYPE,&#xd;&#xa;&#xd;&#xa; t3.MEA_NM      AS MEASURE_TYPE_DESC,&#xd;&#xa;&#xd;&#xa; t1.date_column,&#xd;&#xa;&#xd;&#xa; case&#xd;&#xa;   when trim&#x28;&#x27;&#x24;&#x7b;P_STAT_DATE&#x7d;&#x27;&#x29; is not null and  trim&#x28;&#x27;&#x24;&#x7b;P_STAT_DATE&#x7d;&#x27;&#x29; &#x3c;&#x3e; &#x27;&#x27; and t1.date_range &#x3c;&#x3e; &#x27;4&#x27; -- &#x5feb;&#x7167;&#xd;&#xa;   then &#x27;&#x24;&#x7b;P_STAT_DATE&#x7d;&#x27;&#xd;&#xa;   when t1.date_range in &#x28;&#x27;0&#x27;,&#x27;1&#x27;&#x29; -- &#x6628;&#x65e5;&#x2f;&#x622a;&#x6b62;&#x5230;&#x6628;&#x65e5;&#xd;&#xa;   then DATE_FORMAT&#x28;date_sub&#x28;curdate&#x28;&#x29;,interval 1 day&#x29;,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29;&#xd;&#xa;   when t1.date_range in &#x28;&#x27;2&#x27;,&#x27;3&#x27;&#x29; -- &#x4eca;&#x65e5;&#x2f;&#x622a;&#x6b62;&#x5230;&#x4eca;&#x65e5;&#xd;&#xa;   then DATE_FORMAT&#x28;curdate&#x28;&#x29;,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29;&#xd;&#xa;   when t1.date_range in &#x28;&#x27;4&#x27;&#x29;     -- &#x5feb;&#x7167;&#xd;&#xa;   then DATE_FORMAT&#x28;CURRENT_TIMESTAMP&#x28;&#x29;,&#x27;&#x25;Y-&#x25;m-&#x25;d &#x25;H&#x3a;&#x25;i&#x3a;&#x25;s&#x27;&#x29;&#xd;&#xa; end STAT_DATE,&#xd;&#xa;&#xd;&#xa; case&#xd;&#xa;   when trim&#x28;&#x27;&#x24;&#x7b;P_STAT_DATE&#x7d;&#x27;&#x29; is not null and  trim&#x28;&#x27;&#x24;&#x7b;P_STAT_DATE&#x7d;&#x27;&#x29; &#x3c;&#x3e; &#x27;&#x27; and t1.date_range &#x3c;&#x3e; &#x27;4&#x27; -- &#x5feb;&#x7167;&#xd;&#xa;   then&#xd;&#xa;      case&#xd;&#xa;       when  t1.date_range in &#x28;&#x27;1&#x27;,&#x27;3&#x27;&#x29;&#xd;&#xa;       then  &#x27;1900-01-01&#x27;&#xd;&#xa;       else &#x27;&#x24;&#x7b;P_STAT_DATE&#x7d;&#x27;&#xd;&#xa;      end&#xd;&#xa;   when t1.date_range &#x3d; &#x27;0&#x27; -- &#x6628;&#x65e5;&#xd;&#xa;   then DATE_FORMAT&#x28;date_sub&#x28;curdate&#x28;&#x29;,interval 1 day&#x29;,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29;&#xd;&#xa;   when t1.date_range in &#x28;&#x27;1&#x27;,&#x27;3&#x27;&#x29; -- &#x622a;&#x6b62;&#x5230;&#x6628;&#x65e5;&#x2f;&#x622a;&#x6b62;&#x5230;&#x4eca;&#x65e5;&#xd;&#xa;   then &#x27;1900-01-01&#x27;&#xd;&#xa;   when t1.date_range &#x3d; &#x27;2&#x27; -- &#x4eca;&#x65e5;&#xd;&#xa;   then DATE_FORMAT&#x28;curdate&#x28;&#x29;,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29;&#xd;&#xa;   when t1.date_range in &#x28;&#x27;4&#x27;&#x29;     -- &#x5feb;&#x7167;&#xd;&#xa;   then DATE_FORMAT&#x28;curdate&#x28;&#x29;,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29;&#xd;&#xa; end BEGIN_DATE,&#xd;&#xa;&#xd;&#xa; case&#xd;&#xa;   when trim&#x28;&#x27;&#x24;&#x7b;P_STAT_DATE&#x7d;&#x27;&#x29; is not null and  trim&#x28;&#x27;&#x24;&#x7b;P_STAT_DATE&#x7d;&#x27;&#x29; &#x3c;&#x3e; &#x27;&#x27; and t1.date_range &#x3c;&#x3e; &#x27;4&#x27; -- &#x5feb;&#x7167;&#xd;&#xa;   then DATE_FORMAT&#x28;DATE_ADD&#x28;STR_TO_DATE&#x28;&#x27;&#x24;&#x7b;P_STAT_DATE&#x7d;&#x27;, &#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29;, INTERVAL 1 DAY&#x29;,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29;&#xd;&#xa;   when t1.date_range in &#x28;&#x27;0&#x27;,&#x27;1&#x27;&#x29; -- &#x6628;&#x65e5;&#x2f;&#x622a;&#x6b62;&#x5230;&#x6628;&#x65e5;&#xd;&#xa;   then DATE_FORMAT&#x28;curdate&#x28;&#x29;,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29;&#xd;&#xa;   when t1.date_range in &#x28;&#x27;2&#x27;,&#x27;3&#x27;&#x29; -- &#x4eca;&#x65e5;&#x2f;&#x622a;&#x6b62;&#x5230;&#x4eca;&#x65e5;&#xd;&#xa;   then DATE_FORMAT&#x28;date_add&#x28;curdate&#x28;&#x29;,interval 1 day&#x29;,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29;&#xd;&#xa;   when t1.date_range in &#x28;&#x27;4&#x27;&#x29;     -- &#x5feb;&#x7167;&#xd;&#xa;   then DATE_FORMAT&#x28;date_add&#x28;curdate&#x28;&#x29;,interval 1 day&#x29;,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29;&#xd;&#xa; end END_DATE&#xd;&#xa;&#xd;&#xa;from ETL.ETL_IS_INDEX_CONF t1&#xd;&#xa;left join ETL.ETL_IS_SQL_PATTERN t2&#xd;&#xa;on t1.SQL_PATTERN_ID &#x3d; t2.PATTERN_ID&#xd;&#xa;left join ETL.ETL_IS_MEA_CONF t3&#xd;&#xa;on t1.index_id &#x3d; t3.index_id&#xd;&#xa;left join &#x28;&#xd;&#xa;  select&#xd;&#xa;       INDEX_ID,&#xd;&#xa;       GROUP_CONCAT&#x28;DIM_NM ORDER BY DIM_SEQ&#x29;  AS DIM_NM_LIST,&#xd;&#xa;       GROUP_CONCAT&#x28;DIM_COL ORDER BY DIM_SEQ&#x29; AS DIM_COL_LIST,&#xd;&#xa;       count&#x28;&#x2a;&#x29; as DIM_CNT&#xd;&#xa;  from ETL.ETL_IS_DIM_CONF&#xd;&#xa;  group by INDEX_ID&#xd;&#xa; &#x29; t4&#xd;&#xa;on t1.index_id &#x3d; t4.index_id&#xd;&#xa;WHERE t1.INDEX_ID &#x3d; ifnull&#x28;&#x24;&#x7b;V_INDEX_ID&#x7d;,-1&#x29;&#xd;&#xa;&#x29; tt&#xd;&#xa;&#x29; ttt&#xd;&#xa;order by INDEX_ID&#x3b;</sql>
